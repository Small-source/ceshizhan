    .xuanke {
        .inner {
            width: 1200px;
            margin: 0 auto;
            // overflow: hidden;
        }
        .title {
            height: 130px;
            line-height: 130px;
            font-size: 32px;
            font-weight: bold;
            color: #333;
            text-align: center;
        }
        .banner {
            img {
                width: 100%;
            }
            // height: 480px;
            // background: url(../imgs/xuanke/banner.png) no-repeat center center;
        }
        .solve_problem {
            background-color: #FFFFFF;
            padding-bottom: 40px;
            position: relative;
            // padding-top: 80px;
            color: #333;
            .nav_box {
                width: 1200px;
                height: 140px;
                background-color: #fff;
                border-radius: 5px;
                box-sizing: border-box;
                box-shadow: 0px 0px 10px #dedede;
                display: flex;
                align-items: center;
                justify-content: center;
                color: #fff;
                position: absolute;
                left: 50%;
                top: -60px;
                margin-left: -600px;
                padding: 0 60px;
            }
            .go_peigeng {
                width: 1200px;
                height: 130px;
                box-sizing: border-box;
                padding: 0 100px;
                background-color: #fff;
                border-radius: 5px;
                box-shadow: 0px 0px 10px #dedede;
                display: flex;
                align-items: center;
                justify-content: space-around;
                color: #fff;
                position: absolute;
                left: 50%;
                top: -60px;
                margin-left: -600px;
                .btn {
                    width: 300px;
                    height: 54px;
                    border-radius: 27px;
                    background-color: #377CEE;
                    text-align: center;
                    line-height: 54px;
                    cursor: pointer;
                    font-size: 18px;
                    box-shadow: 0px 5px 5px #0F4FB5;
                    color: #fff;
                    font-size: 16px;
                    &:hover {
                        color: #fff;
                    }
                    &:active {
                        box-shadow: none;
                    }
                }
            }
            .title_box {
                display: flex;
                justify-content: space-between;
                margin-bottom: 20px;
                text-align: center;
                color: #fff;
                line-height: 54px;
                .school {
                    width: 278px;
                    height: 66px;
                    background: url(../imgs/xuanke/bg_xuexiao.png);
                }
                .student {
                    width: 892px;
                    height: 66px;
                    background: url(../imgs/xuanke/bg_student.png);
                }
            }
            .main_box {
                display: flex;
                justify-content: space-between;
                padding-bottom: 5px;
                li {
                    padding: 20px 15px;
                    display: flex;
                    background-color: #fff;
                    font-size: 14px;
                    border-radius: 2px;
                    width: 276px;
                    box-sizing: border-box;
                    align-items: center;
                    box-shadow: 0px 0px 20px rgba(222,222,222,.5);
                    img {
                        margin-right: 15px;
                    }
                }
            }
        }
        .kexuexuanke {
            background-color: #F4F6F9;
            padding-bottom: 50px;
            .main_box {
                display: flex;
                justify-content: space-between;
                color: #333;
                li {
                    position: relative;
                    width: 366px;
                    height: 490px;
                    box-sizing: border-box;
                    padding-top: 300px;

                    .num {
                        text-align: center;
                        height: 65px;
                        overflow: hidden;
                    }
                    .main_title {
                        text-align: center;
                        font-size: 20px;
                        font-weight: 600;
                        color: #333;
                        line-height: 50px;
                    }
                    .main_detail {
                        padding: 0 60px;
                        font-size: 14px;
                        text-align: center;
                        color: #333;
                        line-height: 20px;
                    }
                }
            }
        }
        .our_advantage {
            padding-bottom: 30px;
            .content {
                background: url(../imgs/xuanke/bg_youshi.png) no-repeat center center;
                height: 366px;
                ul {
                    display: flex;
                    justify-content: space-between;
                    height: 366px;
                    position: relative;
                    top: 0px;
                    z-index: 10;
                    li {
                        box-sizing: border-box;
                        width: 240px;
                        height: 366px;
                        padding-top: 70px;
                        color: #fff;
                        text-align: center;
                        .small_title {
                            line-height: 60px;
                            text-align: center;
                            font-size: 20px;
                        }
                        .line {
                            width: 95px;
                            border: 1px solid #fff;
                            margin: 0 auto;
                        }
                        .small_detail {
                            padding: 0 40px;
                            padding-top: 25px;
                            font-size: 14px;
                            text-align: left;
                            text-align: center;
                        }
                        .img_box {
                            height: 40px;
                        }
                        &:nth-child(1) {
                            .img_box {
                                background: url(../imgs/xuanke/icon_xianshang.png) no-repeat center center;
                            }
                        }
                        &:nth-child(2) {
                            .img_box {
                                background: url(../imgs/xuanke/icon_jiejuefangan.png) no-repeat center center;
                            }
                        }
                        &:nth-child(3) {
                            .img_box {
                                background: url(../imgs/xuanke/icon_shizi.png) no-repeat center center;
                            }
                        }
                        &:nth-child(4) {
                            .img_box {
                                background: url(../imgs/xuanke/icon_genzong.png) no-repeat center center;
                            }
                        }
                        &:hover {
                            position: relative;
                            z-index: 2;
                            box-shadow: 0px 0px 35px rgba(167,169,180,.2);
                            background-color: #fff;
                            color: #333;
                            .small_title {
                                color: #333;
                            }
                            .line {
                                border: 1px solid #377BED;
                            }
                            .small_detail {
                                 color: #333;
                            }
                            &:nth-child(1) {
                                .img_box {
                                    background: url(../imgs/xuanke/icon_xianshanglan.png) no-repeat center center;
                                }
                            }
                            &:nth-child(2) {
                                .img_box {
                                    background: url(../imgs/xuanke/icon_jiejuefanganlan.png) no-repeat center center;
                                }
                            }
                            &:nth-child(3) {
                                .img_box {
                                    background: url(../imgs/xuanke/icon_shizilan.png) no-repeat center center;
                                }
                            }
                            &:nth-child(4) {
                                .img_box {
                                    background: url(../imgs/xuanke/icon_genzonglan.png) no-repeat center center;
                                }
                            }
                        }
                    }
                    .active {
                        position: relative;
                        z-index: 2;
                        box-shadow: 0px 0px 35px rgba(167,169,180,.2);
                        background-color: #fff;
                        color: #333;
                        .small_title {
                            color: #333;
                        }
                        .line {
                            border: 1px solid #377BED;
                        }
                        .small_detail {
                             color: #333;
                        }
                    }

                }
            }
        }
        .gongnengjieshao {
            .inner {
                display: flex;
            }
            .middle {
                flex: 1;
                display: flex;
                justify-content: center;
            }
            .main_box {
                padding-top: 60px;
                .title_box {
                    width: 186px;
                    height: 42px;
                    line-height: 42px;
                    background-color: #3492f9;
                    color: #fff;
                    text-align: center;
                    border-radius: 21px;
                    margin-bottom: 25px;
                }
                ul {
                    li {
                        display: flex;
                        width: 236px;
                        margin-bottom: 20px;
                        img {
                            margin-right: 15px;
                            display: block;
                            width: 38px;
                            height: 38px;
                        }
                        p {
                            line-height: 1.5em;
                            color: #333;
                            font-size: 14px;
                        }
                    }
                }
            }
        }
        .jiehe {
            .content {
                color: #fff;
                height: 595px;
                background: url(../imgs/xuanke/bg_shixianxueshengxuan.png) no-repeat center center;
                .inner {
                    padding-top: 60px;
                    height: 595px;
                    box-sizing: border-box;
                    display: flex;
                    .main_box {
                        width: 220px;
                        .title_box {
                            margin-bottom: 45px;
                            span {
                                display: inline-block;
                                line-height: 60px;
                                font-size: 30px;
                                border-bottom: 1px solid #fff;
                            }
                        }
                        ul {
                            li {
                                display: flex;
                                align-items: center;
                                font-size: 16px;
                                margin-bottom: 35px;
                                img {
                                    margin-right: 25px;
                                }
                            }
                        }
                    }
                    .inner_right {
                        color: #333;
                        .title_box {
                            span {
                                border-bottom: 1px solid #333;
                            }
                        }
                    }
                    .middle {
                        flex: 1;
                        display: flex;
                        justify-content: center;
                        padding-top: 30px;
                        img {
                            width: 357px;
                            height: 281px;
                        }
                    }
                }
            }
        }
        .fuwuliucheng {
            height: 509px;
            background: url(../imgs/xuanke/icon_fuwuliucheng.png) no-repeat center center;
            .main_box {
                padding: 150px 60px 0px 60px;
                display: flex;
                justify-content: space-between;
                li {
                    color: #333;
                    width: 190px;
                    text-align: center;
                }
                .main_title {
                    font-size: 18px;
                    line-height: 50px;
                    font-weight: bold;
                }
                .mian_detail {
                    font-size: 14px;
                    line-height: 22px;
                }
            }
        }
    }