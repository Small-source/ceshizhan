<script>
    $(function () {
        $.ajax({
            type: "post",
            url: port+"/article/getArticleList",
            data:{
                cateId: 1,
                pageSize: 10,
                totalPage: 1
            },
            dataType:'json',
            success: function(res) {
                if(res.code==2000){
                    if(res.data.list){
                        var data=res.data.list
                        ul=$('.footerNavList')
                        data.forEach(function (item) {
                            var $li=$('<li></li>')
                            var $a = $('<a></a>').html(item.title).attr('href','路径?id='+item.id).appendTo($li)
                            ul.append($li)
                        })
                    }
                }
            },
            error : function(e){
            }
        });
    })
</script>
