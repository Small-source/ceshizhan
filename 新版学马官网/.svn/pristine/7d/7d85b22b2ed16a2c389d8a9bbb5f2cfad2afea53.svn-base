.head {
  background: url("../imgs/home/banner.png") center center no-repeat;
  width: 100%;
  height: 540px;
}
.parts {
  width: 1200px;
  margin: 0 auto;
}
.part1 {
  background: url("../imgs/home/icon_shengxuefuwu.png") center center no-repeat;
  width: 100%;
  height: 880px;
}
.part1 .tops {
  width: 100%;
  height: 150px;
  border-radius: 20px;
  background: #fff;
  position: relative;
  top: -54px;
  box-shadow: 0 20px 20px #F5F5F5;
}
.part1 .tops > li {
  width: 20%;
  height: 100%;
  text-align: center;
  float: left;
  font-size: 18px;
  color: #666666;
  padding-top: 25px;
  cursor: pointer;
}
.part1 .tops > li:hover span {
  transform: scale(1.2);
}
.part1 .tops > li > p {
  font-size: 18px;
  padding: 10px 0 18px;
  color: #666666;
  text-align: left;
  text-indent: 57px;
}
.part1 .tops > li > span {
  font-size: 45px;
  font-weight: bolder;
  color: #3186F7;
  letter-spacing: 0;
  display: inline-block;
  transition: 0.1s;
}
.part1 .tops > li > a {
  color: #666;
  position: relative;
  top: -2px;
}
.part1 .dajiantou {
  display: block;
  margin: 46px auto 0;
}
.title {
  font-size: 33px;
  text-align: center;
  font-weight: 600;
}
.title span {
  font-size: 18px;
  color: #fff;
}
.xueye {
  width: 100%;
  height: 320px;
  border-bottom: 1px solid #FFFFFF;
  background: url("../imgs/home/icon_lanbeijing.png") center center no-repeat;
}
.xueyelist {
  width: 954px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  height: 100%;
}
.xueyelist li {
  width: 225px;
  height: 100%;
  position: relative;
}
.xueyelist li p {
  text-align: center;
  line-height: 76px;
  font-size: 26px;
  color: #fff;
}
.xueyelist li .p {
  position: absolute;
  left: 0;
  bottom: -76px;
  color: #333;
  font-size: 20px;
  width: 100%;
}
