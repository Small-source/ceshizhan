<!--pages/service/service.wxml-->
<view class="page">
  <view class="header">
    <view class="lis top">
        <image src="https://m.xuematech.com/applet/img/service/icon_dingwei.png"></image>
        <view class="s_title">联系地址：</view>
        <view>
          <text>{{agentInfo.province}}{{agentInfo.city}}{{agentInfo.addressInfo}}</text>
        </view>
    </view>
    <view class="lis">
        <image src="https://m.xuematech.com/applet/img/service/icon_lianxifangshi.png"></image>
        <view class="s_title">联系方式：</view>
        <view>
          <text bindtap="callPhone" class="callPhone">{{agentInfo.serviceTel}}</text>
        </view>
    </view>
    <view class="lis">
        <image src="https://m.xuematech.com/applet/img/service/icon_guanwangdizhi.png"></image>
        <view class="s_title">
          官网地址：
        </view>
        <view><text selectable="true">{{agentInfo.oem}}</text></view>
    </view>
  </view>
  <navigator class="main" wx:for="{{cardData}}" wx:key="index" url="/pages/service/serviceDetail?id={{index}}">
    <image src="{{item.imgUrl}}"></image>
    <view class="right">
      <view class="title">{{item.title}}</view>
      <view class="detail" wx:for="{{item.detail}}" wx:key="index" wx:for-item="child">{{child}}</view>
    </view>
  </navigator>
</view>
