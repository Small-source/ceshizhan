/* miniprogram/pages/matchingSchools/matchingSchools.wxss */
@import  "../public/public.wxss";
.head{
  height: 92rpx;
  width: 100%;
  border-bottom:2rpx solid #CFCFD0;
  background: #fff;
  display: flex;
  justify-content: space-between;
  position: fixed;
  z-index: 999;
}
.classification{
  width: 50%;
  height: 100%;
  line-height: 92rpx;
  text-align: center;
}
.classification text{
  line-height: 92rpx;
  font-size:30rpx;
  color:#333333;
  font-weight: bold
}
.classification image{
  width: 24rpx;
  height: 14rpx;
  position: relative;
  top:-4rpx;
  left:4rpx;
}
.Yuanxiao{
  position: fixed;
  top:92rpx;
  left:0;
  background: #fff;
  padding: 0 30rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: wrap;
  z-index: 999;
}
.list{
  width: 148rpx;
  height: 60rpx;
  background: #F1F2F5;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  margin-top: 30rpx;
  border-radius: 6rpx;
}
#clickBlue{
  background: #3979FD;
  color: white;
}
.submit{
  width:166rpx;
  height: 64rpx;
  font-size: 30rpx;
  color: white;
  text-align: center;
  line-height: 60rpx;
  background: #3979FD;
  border-radius: 30rpx;
  flex-shrink: 0;
  margin: 48rpx auto;
}
.toudangxian{
  width: 280rpx;
  float: right;
  height: 68rpx;
  line-height: 68rpx;
  display: flex;
  justify-content: space-between;
  margin-right: 30rpx;
}
.line{
  height: 24rpx;
  width: 2rpx !important;
  background: #C6D2F6;
  position: relative;
  top:24rpx;
}
.toudangxian>view {
  color:#3569F8;
  font-size: 24rpx;
}
.toudangxian>.image>image{
  display: block;
  width: 32rpx;
  height: 32rpx;
  position: relative;
  top:16rpx;
}
.viewList{
  margin-bottom: 8rpx;
  background: #fff;
  overflow: hidden;
  padding: 30rpx;
}
.viewLeft{
  float: left;
  box-sizing: border-box;
}
.viewMiddle{
  float: left;
  max-width:440rpx;
  position: relative;
}
.viewLeft>image{
  width: 64rpx;
  height: 64rpx;
  position: relative;
  top:8rpx;
}
.viewMiddle>view{
  font-size: 28rpx;
  color:#101010;
  font-weight: bold;
  margin-left: 14rpx;
  min-height: 38rpx;
}
.viewMiddleChild>view{
  float: left;
  font-size: 20rpx;
  color:#8A8A8A;
  border:1px solid #9CBCFD;
  line-height: 28rpx;
  padding: 0 16rpx;
  border-radius: 14rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
  font-weight: normal;
}
.viewRight{
  width: 148rpx;
  float: right;
  height: 68rpx;
  line-height: 68rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 6rpx;
}
.viewRight>view{
  color:#3569F8;
  font-size: 24rpx;
  width: 60rpx;
  text-align: center;
}
.toudang{
  position: fixed;
  width: 100%;
  top:92rpx;
  right:0;
  background: #F6F6F6;
  z-index: 10;
}
.bottom{
  text-align: center;
  font-size: 24rpx;
  color:#A6A6A6;
  line-height: 88rpx;
}
.bottom>image{
  width: 28rpx;
  height:28rpx;
  position: relative;
  top:4rpx;
}
.none{
  width: 330rpx;
  height: 330rpx;
  position: fixed;
  top:50%;
  left:50%;
  margin-top: -165rpx;
  margin-left: -165rpx;
}
.noneBox{
  position: fixed;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background:#fff;
}
.zhange{
  height: 160rpx;
}
.dingweis{
  position: relative;
}
.dingwei{
  display: inline-block;
  z-index: 5;
  font-size: 22rpx;
  color:#A8A8A8;
  font-weight: normal;
}
.dingwei image{
  width: 15rpx;
  height: 23rpx;
  margin-left: 5rpx;
}