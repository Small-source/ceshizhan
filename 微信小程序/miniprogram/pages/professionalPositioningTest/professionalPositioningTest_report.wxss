/* miniprogram/pages/professionalPositioningTest/professionalPositioningTest_report.wxss */
view,text{
  box-sizing: border-box;
  margin:0;
  padding: 0;
}
.part{
  width: 750rpx;
  min-height: 1334rpx;
  overflow: hidden;
   background-image: url("https://m.xuematech.com/applet/img/zhuanyedingwei/bg.png");
