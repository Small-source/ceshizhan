.pdfContent {
  width: 900px;
  //position: absolute;
  //top: 50px;
  //left: 0;
  color: #000;
  font-size: 17px;
  margin-left: 10px;
  margin-top: -50px;
  //margin: 20px auto;
  overflow: hidden;
  p {
    line-height: inherit;
  }
  .pdf-wrapper {
    width: 100%;
    float: left;
  }
  .font-blue {
    color: #1654a1
  }
  .font-medium {
    font-size: 20px;
  }
  .m-b20 {
    margin-bottom: 20px;
  }

  .m-b15 {
    margin-bottom: 15px;
  }
  .pdf-page {
    width: 100%;
    height: 1340px;
    padding: 36.42px 0;
    background: #fff;
    float: left;
    box-sizing: border-box;
    .pdf-header {
      width: 90%;
      height: 40px;
      border-bottom: 1px solid #000;
      overflow: hidden;
      margin: 0 auto 10px;
      line-height: 40px;
      box-sizing: border-box;
      .pdf-page-seq {
        width: 38px;
        height: 38px;
        background: url("~@/assets/pdf/seq-bg.png") no-repeat center / 100% 100%;
        text-align: center;
        color: #fff;
        margin-left: 5px;
      }
    }
    .pdf-content {
      width: 90%;
      box-sizing: border-box;
      border: 1px solid #000;
      padding: 20px 2%;
      height: 1100px;
      margin: 0 auto;
      .pdf-title {
        height: 40px;
        line-height: 40px;
        width: 90%;
        margin: 20px auto;
        text-align: center;
        background: #006699;
        color: #fff;
      }
      .pdf-title-2 {
        background: #068b50;
      }
      .page-one-title {
        display: block;
        margin: 60px auto 0;
        width: 70%;
      }
      .page-one-logo {
        display: block;
        margin: 150px auto 150px;
        width: 100px;
      }
      .page-one-name {
        width: 390px;
        font-size: 17px;
        margin: 0 auto;
        li {
          width: 100%;
          float: left;
          height: 20px;
          line-height: 20px;
          margin-bottom: 20px;
          p {
            float: left;
          }
          p:last-child {
            height: 20px;
            width: 300px;
            border-bottom: 1px solid #000;
          }
        }
      }
      .page-one-bottom {
        width: 80%;
        margin-left: 10%;
        text-align: center;
        line-height: 40px;
        float: left;
        margin-top: 60px;
        img {
          display: block;
          width: 100%;
        }
      }
      .pdf-page-title {
        font-size: 30px;
        text-align: center;
        margin: 0 auto 20px;
        width: auto;
        span {
          border-bottom: 1px solid #1654a1;
        }
      }
      .pdf-paragraph {
        text-indent: 2rem;
        line-height: 40px;
      }

      .pdf-border-paragraph {
        width: 100%;
        padding: 10px;
        border: 1px solid #000;
        box-sizing: border-box;
        line-height: 40px;
        float: left;
        font-size: 17px;
        p {
          text-indent: 2rem;
        }
        .blue-title {
          width: 102.5%;
          margin-left: -1.3%;
          height: 50px;
          background: #e1f5ff;
          font-size: 20px;
          text-indent: 26px;
          margin-bottom: 0px;
          line-height: 50px;
          margin-top: -10px;
        }
        .left-block {
          width: 100%;
          float: left;
          .pull-left {
            width: 10px;
            height: 10px;
            background: #99148d;
            margin-top: 13px;
          }
          .text-container {
            margin-left: 17px;
            p {
              text-indent: 0;
              margin-bottom: 0;
            }
          }
        }
      }
      .pdf-mins-height{
        line-height: 33px;
      }
      .pdf-min-height{
        line-height: 34px;
      }
      .font-medium.pdf-paragraph {
        line-height: 35px;
      }
      .pbp-height-medium {
        line-height: 30px;
      }
      .page-2-user {
        width: 600px;
        margin: 160px auto 0;
        overflow: hidden;
        line-height: 30px;
        .text-center {
          font-size: 25px;
          margin-bottom: 20px;
        }
        ul {
          border: 1px solid #000;
          float: left;
          width: 100%;
          box-sizing: border-box;
        }
        li {
          width: 100%;
          float: left;
          height: 30px;
          line-height: 30px;
          p {
            border: 1px solid #000;
            border-top: 0;
            float: left;
            box-sizing: border-box;
            width: 50%;
            border-left: 0;
            span {
              width: 200px;
              display: block;
              text-align: right;
            }
          }
          p:last-child {
            border-right: 0;
            text-align: center;
          }
        }
      }
      .page-2-user-2 {
        margin-top: 440px !important;
      }
      .page-3-title {
        display: block;
        width: 250px;
        margin: -20px auto 40px;
      }
      .page-3-content {
        width: 90.3%;
        margin: 0 auto;
        img {
          display: block;
          width: 100%;
        }
        .page-3-main {
          position: absolute;
          width: 100%;
          top: 4px;
          left: 0;
          li {
            width: 100%;
            line-height: 32px;
            text-align: right;
          }
        }
        .page-4-main {
          top: 13px;
        }
      }
      .pdf-blue-title {
        width: 104.6%;
        height: 50px;
        margin-left: -2.25%;
        background: #a1c5f1;
        color: #333;
        line-height: 50px;
        font-size: 22px;
        margin-bottom: 20px;
        float: left;
        box-sizing: border-box;
        padding: 0 2.5%;
        display: flex;
        align-items: center;
        img {
          width: 25px;
          margin-right: 8px;
        }
      }
      .pdf-yellow-title {
        background: #ffc98a;
      }
      .pdf-purple-title {
        background: #f1cdff;
      }
      .page-5-biao {
        width: 600px;
        margin: 0 auto;
        position: relative;
        overflow: hidden;
        img {
          display: block;
          width: 100%;
        }
        .biao-content {
          width: 300px;
          position: absolute;
          top: 65.5px;
          left: 301px;
          z-index: 10;
          ul {
            li {
              width: 100px;
              height: 30px;
              background: #99148d;
              margin-bottom: 20.5px;
            }
          }
        }
        .biao-font {
          width: 90px;
          position: absolute;
          line-height: 50px;
          top: 56px;
          font-size: 14px;
          li {
            height: 30px;
            line-height: 30px;
            width: 90px;
            text-align: center;
            margin: 10px 0;
            float: left;
          }
          li.active {
            background: #99148d;
            color: #fff
          }
        }
        .biao-left-font {
          left: 20px;
        }
        .biao-right-font {
          right: 20px;
        }
        .conclusion {
          width: 100%;
          text-align: center;
          position: absolute;
          top: 285px;
          p {
            margin-bottom: 5px;
          }
          P.name {
            color: #99148d;
          }
          .name1 {
            font-weight: bold;
          }
        }
      }
      .page-9-biao {
        margin-top: 50px;
        .biao-content {
          top: 97px;
          left: 301px;
        }
        .biao-font {
          top: 87px;
        }
      }
      .page-17-biao {
        .line-list {
          position: absolute;
          top: 83px;
          width: 496px;
          left: 142px;
          .line-wrapper {
            overflow: visible !important;
            strong {
              font-size: 14px;
              position: absolute;
              top: -1px;
              z-index: 10;
            }
          }
          .line-wrapper:nth-child(5n+0) {
            margin-bottom: 31px;
          }
          .line {
            width: 150px;
            height: 12px;
            background: #aad0fe;
            position: relative;
            margin-bottom: 4px;

            span {
              position: absolute;
              width: 2px;
              height: 12px;
              left: 50%;
              top: 0;
              margin-left: -1px;
              background: #289aff;
            }
            strong {
              font-size: 14px;
              display: block;
              position: relative;
              z-index: 10;
              margin-top: 0px;
              top: -2px
            }
          }
        }
      }
      .page-19-biao {
        .biao-content {
          height: 410px;
          top: 35px;
          left: 67px;
          width: 640px;
          .score-item {
            width: 52px;
            float: left;
            height: 100%;
            position: relative;
            margin-right: 1.3px;
            .score-item-text {
              width: 100%;
              float: left;
              text-align: center;
              height: 22px;
              line-height: 23px;
              font-size: 12px;
              color: #444;
            }
            .score-line-wrapper {
              width: 100%;
              float: left;
              height: 370px;
              position: relative;
              margin-top: 16px;
              .line-score {
                width: 16px;
                position: absolute;
                bottom: 0px;
                left: 50%;
                height: 120px;
                margin-left: -8px;
                background: #00A2FF;
                .line-score-norm {
                  width: 26px;
                  position: absolute;
                  bottom: 10px;
                  height: 60px;
                  left: -5px;
                  background: rgba(0, 0, 0, 0.4);
                }

              }
              .line-score.b-blue {
                background: #0869bb;
              }
              .line-score.b-green {
                background: #0aa70a;
              }
              .line-score.b-yellow {
                background: #ffe21c;
              }
              .line-score.b-orange {
                background: #f49a0b;
              }
              .line-score.b-red {
                background: #ff001e;
              }
            }
          }
        }
      }
      .jixing {
        width: 700px;
        box-sizing: border-box;
        height: 56px;
        border: 1px solid #ff0000;
        padding: 2px;
        margin: 0 auto 90px;
        overflow: hidden;
        .jx-content {
          width: 100%;
          height: 50px;
          background: #ff0000;
          line-height: 50px;
          text-align: center;
          color: #fff;

          font-size: 20px;
          p:first-child {
            width: 300px;
            float: left;
          }
          p:last-child {
            width: 300px;
            float: left;
          }
        }
      }
      .recommend-major {
        height: 40px;
        overflow: hidden;
        text-align: center;
        line-height: 40px;
        width: 500px;
        margin: 0 auto;
        p {
          width: 50%;
          float: left;
        }
      }
    }

    .pdf-footer {
      width: 90%;
      height: 40px;
      border-top: 1px solid #000;
      margin: 10px auto 0;
      overflow: hidden;
      box-sizing: border-box;
      line-height: 25px;
    }
  }

}

.table-container {
  width: 100%;
  float: left;
  margin-bottom: 20px;
  .table-title {
    width: 100%;
    float: left;
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 20px;
    font-weight: bold;
  }
  table {
    width: 100%;
    border: 2px solid #000;
    box-sizing: border-box;
    text-align: center;
    font-size: 14px;
    border-collapse: collapse;
    .score-line {
      height: 20px;
      width: 355px;
      .line {
        height: 16px;
        background: #0869bb;
      }
      .line.b-blue {
        background: #0869bb;
      }
      .line.b-green {
        background: #0aa70a;
      }
      .line.b-yellow {
        background: #ffe21c;
      }
      .line.b-orange {
        background: #f49a0b;
      }
      .line.b-red {
        background: #ff001e;
      }
      .norm-line {
        width: 60px;
        height: 26px;
        background: #000;
        opacity: 0.2;
        margin-top: -21px;
      }
      .black-line {
        width: 14px;
        height: 26px;
        background: #000;
        position: absolute;
        top: -5px;
        left: 50%;
        margin-left: -7px;
      }
    }
    .score-text {
      display: flex;
      width: 100%;
      margin: 5px auto;
      justify-content: space-between;
    }
    tr {

    }
    th {
      font-weight: normal;
      border: 1px solid #000;
      height: 35px;
      line-height: 35px;
    }
    th, td.bg-blue {
      background: #c3deff;
    }
    td {
      height: 35px;
      line-height: 35px;
      border: 1px solid #000;
    }
  }
}

.table-25-container {
  table {
    .score-line {
      height: 30px;
      width: 409px;
      .line {
        height: 30px;
        background: #4a9bff;
        position: relative;
        span {
          display: block;
          position: absolute;
          top: 0;
          left: 0;
          height: 30px;
          background: rgba(0, 0, 0, 0.2);
          width: 100px;
          opacity: 0.5;
        }
      }
    }
    td {
      line-height: 25px;
      padding: 10px 0;
    }
  }
}

.table-29-container table td {
  height: 30px;
  line-height: 30px;
}

.table-55-container {
  table {
    td {
      height: 60px;
    }
  }
}

.table-container table .score-line {
  height: 16px;
}

.table-49-container {
  table {
    td {
      .score-line {
        width: 350px;
