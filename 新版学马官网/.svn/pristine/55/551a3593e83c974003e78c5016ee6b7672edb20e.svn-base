        .big_title {
            color: #fff;
        }
        .lis {
            display: flex;
            justify-content: space-between;
            flex-wrap: wrap;
            .lid {
                box-sizing: border-box;
                width: 520px;
                height: 138px;
                padding: 0px 30px;
                padding-top: 20px;
                border-radius: 20px;
                margin-bottom: 45px;
                background-color: #fff;
                box-shadow: 10px 10px 0px #E7252E;
                .top_box {
                    display: flex;
                    margin-bottom: 10px;
