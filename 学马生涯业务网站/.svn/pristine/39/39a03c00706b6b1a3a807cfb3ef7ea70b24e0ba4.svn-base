            }
            .radio.active {
              background: url("~@/assets/user/radio-on.png") no-repeat left center / 20px auto;
            }
            span.label {
              display: block;
              float: left;
              margin: 0px 5px;
              min-width: 40px;
            }
            .heightSite {
              width: 100%;
              height: 10px;
              float: left;
            }
            select.select {
              width: 270px;
              height: 30px;
              box-sizing: border-box;
              border: 1px solid #999;
              background: none;
            }
          }

        }
        .grey-bg {
          background: #eee;
          width: 100%;
          float: left;
          margin-top: 15px;
          .m-in {
            width: 450px;
            float: left;
            margin: 15px 0 15px 30px;
          }
        }
      }
    }
  }
}

