    .footer {
        height: 106px;
        padding-top: 30px;
        background-color: #222222;
        color: #fff;
        .left {
            float: left;
            width: 860px;
            p {
                font-size: 15px;
                line-height: 36px;
            }
        }
        .right {
            font-size: 24px;
