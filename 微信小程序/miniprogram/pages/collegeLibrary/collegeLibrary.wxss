/* pages/collegeLibrary/collegeLibrary.wxss */
view {
  color: #333;
}
.title {
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
}
.noMore {
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
}
.btn {
  width: 164rpx;
  height: 64rpx;
  border-radius: 32rpx;
  background-color: #3979FD;
  color: #fff;
  line-height: 64rpx;
  font-size: 28rpx;
  text-align: center;
  margin: 0 auto;
}
.topFloat {
  background-color: #fff;
  margin-bottom: 10rpx;
  box-shadow: 0 0 5rpx #dedede;
  height: 200rpx;
  box-sizing: border-box;
  padding-top: 30rpx;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 2;
}
.topFloat .searchBox {
  width: 690rpx;
  height: 64rpx;
  border: 1px solid #DEDEDE;
  margin: 0 auto;
  border-radius: 32rpx;
  display: flex;
  align-items: center;
}
.topFloat .searchBox input {
  flex: 1;
  padding-left: 30rpx;
}
.topFloat .searchBox view {
  width: 40rpx;
  height: 40rpx;
  padding-right: 30rpx;
}
.topFloat .checkBox {

  position: relative;
}
.topFloat .checkBox .tabBox{
  padding-bottom: 30rpx;
  padding-top: 30rpx;
  align-items: center;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
  box-sizing: border-box;
}
.topFloat .checkBox .tabBox view {
  width: 33%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 1px solid #F5F5F5;
}
.topFloat .checkBox .tabBox .activeTab {
  color: #3979FD;
}
.topFloat .checkBox .tabBox .activeTab image{
  color: #3979FD;
  transform: rotate(180deg)
}
.topFloat .checkBox .tabBox view image {
  width: 22rpx;
  height: 12rpx;
}
.topFloat .checkBox .childBox {
  border-top: 1px solid #A0A0A1;
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
  padding: 30rpx 30rpx 0 30rpx;
  justify-content: space-between;
  align-items: baseline;
  flex: start;
}
.topFloat .checkBox .childBox view {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 22rpx;
  background-color: #F1F2F5;
  text-align: center;
  margin-bottom: 30rpx;
  border-radius: 5rpx;
}
.topFloat .checkBox .btnBox {
  padding-bottom: 30rpx;
  background-color: #fff;
}
.topFloat .checkBox  .nomall view{
  width: 150rpx;
}
.topFloat .checkBox .tagBox view {
  padding: 0 50rpx;
}
.topFloat .checkBox .childBox .active {
  background-color: #3979FD;
  color: #fff;
}
.content {
  padding-top: 210rpx;
  background-color: #F6F4F2;
}
.main {
  display: flex;
  padding: 30rpx;
  align-items: center;
  margin-bottom: 10rpx;
  background-color: #fff;
}
.main image {
  width: 64rpx;
  height: 64rpx;
  margin-right: 20rpx;
}
.main .mainRight {
  flex: 1;
  position: relative;
}
.main .top {
  display: flex;
  align-items: center;
}
.main .top .title {
  margin-right: 30rpx;
}
.main .top .type {
  display: flex;
  font-size: 22rpx;
  align-items: center;
}
.main .top .type image {
  width: 26rpx;  
  margin-right: 5rpx;
}
.main .bottom {
  /* display: flex; */
  margin-top: 20rpx;
  width: 100%;
  /* overflow: hidden; */
  font-size: 20rpx;
  color: #666666;
  /* height: 32rpx; */
}
.main .bottom .tag {
  padding: 0 12rpx;

  border: 1px solid #3979FD;
  line-height: 30rpx;

  margin-right: 5rpx;
  float: left;
  border-radius: 15rpx;
}
.main .mainRight .address {
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -16rpx;
  height: 32rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.main .mainRight .address image {
  width: 15rpx;
  height: 23rpx;
  margin-right: 5rpx;
}
mark {
  width: 100%;
  height: 100%;
}
.none{
  width: 330rpx;
  height: 330rpx;
  position: fixed;
  top:50%;
  left:50%;
  margin-top: -165rpx;
  margin-left: -165rpx;
}
.noneBox{
  position: fixed;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background:#fff;
}
