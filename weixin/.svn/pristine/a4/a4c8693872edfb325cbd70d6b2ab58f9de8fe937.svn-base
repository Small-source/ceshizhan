.evaluation_report {
     width: 100%;
}
 .evaluation_report * {
     font-size: 0.22rem;
     color: #333;
}
 .evaluation_report .get_pdf {
     height: 0rem;
     width: 100%;
     position: fixed;
     top: 30%;
}
 .evaluation_report .get_pdf .inner {
     width: 7.2rem;
     margin: 0 auto;
     position: relative;
}
 .evaluation_report .get_pdf .inner .btn_pdf {
     position: absolute;
     padding: 0.04rem 0.08rem;
     right: -0.32rem;
     width: 0.16rem;
     text-align: center;
     background-color: #2c8ffc;
     color: #fff;
     cursor: pointer;
     border-radius: 0.04rem;
}
 .evaluation_report .pdf {
     width: 100%;
}
 .evaluation_report .pdf .page_shadow {
     -webkit-box-shadow: 0rem 0rem 0.08rem #666;
     -ms-box-shadow: 0rem 0rem 0.08rem #666;
     -moz-box-shadow: 0rem 0rem 0.08rem #666;
     min-height: 13.34rem;
     width: 100%;
     margin-bottom: 0.16rem;
}
 .evaluation_report .pdf .page_cover {
     height: 10.2rem;
     padding-top: 3.14rem;
     background-color: #fff;
     background: url(../img/diagnosis/reportbgp.png) no-repeat;
     background-size: 100% 100%;
}
.evaluation_report .pdf .page_cover .header {
     margin-bottom: 2.20rem;
}
.evaluation_report .pdf .page_cover .header h3 {
     margin: 0;
     text-align: center;
     line-height: 0.66rem;
     font-size: 0.54rem;
     color: #168375;
     margin-bottom: 0.24rem;
}
 .evaluation_report .pdf .page_cover .header .detail {
     line-height: 0.4rem;
     text-align: center;
     font-size: 0.3rem;
     color: #333;
}
 .evaluation_report .pdf .page_cover .main {
     width: 4rem;
     margin: 0 auto;
     margin-bottom: 2.4rem;
}
 .evaluation_report .pdf .page_cover .main p {
     line-height: 0.48rem;
     font-size: 0.26rem;
     font-weight: 600;
     color: #333;
}
 .evaluation_report .pdf .page_cover .main .name {
     display: flex;
     justify-content: center;
 }
  .evaluation_report .pdf .cover_warning {
     padding: 0 0.3rem;
  }
  .evaluation_report .pdf .cover_warning li {
     line-height: 0.32rem;
     padding-left: 0.35rem;
     font-size: 0.22rem;
     margin-bottom: 0.16rem;
  }
  .evaluation_report .pdf .cover_warning li .img_box{
     float: left;
     margin-left: -0.35rem;
     width: 0.32rem;
     height: 0.32rem;
     background: url(../img/zhuyili/icon_xingxing.png) no-repeat center center;
     background-size: 0.28rem 0.28rem;
  }
 .evaluation_report .pdf .page_cover .bottom {
     padding-top: 0.4rem;
     color: #fff;
}
 .evaluation_report .pdf .page_cover .bottom p {
     line-height: 0.24rem;
     text-align: center;
     font-size: 0.128rem;
     color: #fff;
}
 .evaluation_report .pdf .page {
     min-height: 11.64rem;
     padding-top: 1.7rem;
     background: url(../img/diagnosis/reportbgp.png) no-repeat;
     background-size: 100% 100%;
     background-color: #fff;
     position: relative;
}
 .evaluation_report .pdf .page .header {
     text-align: center;
     color: #168375;
     font-size: 0.56rem;
     line-height: 0.56rem;
     margin-bottom: 0.35rem;
     font-weight: normal;
}
 .evaluation_report .pdf .page .mulu {
     margin-bottom: 0.86rem;
 }
 .evaluation_report .pdf .page .header_detail {
     text-align: center;
     line-height: 0.48rem;
}
 .evaluation_report .pdf .page .content {
     padding: 0.24rem 0.3rem;
     padding-top: 0rem;
}
 .evaluation_report .pdf .page .content .big_title_box {
     display: flex;
     line-height: 0.28rem;
     margin-bottom: 0.3rem;
}
 .evaluation_report .pdf .page .content .big_title_box .big_title {
     position: relative;
     font-size: 0.28rem;
     color: #000;
     padding-right: 4px;
}
 .evaluation_report .pdf .page .content .big_title_box .big_title span {
     height: 0.28rem;
     font-size: 0.28rem;
     line-height: 0.2.8rem;
     color: #168375;
     font-weight: 600;
     padding-right: 0.2rem;
}
 .evaluation_report .pdf .page .content .big_title_box .line {
     flex: 1;
     border-bottom: 1px dashed #000;
     margin-bottom: 2px;
}
 .evaluation_report .pdf .page .content .big_title_box .page_number {
     padding-left: 0.08rem;
     font-size: 0.24rem;
}
 .evaluation_report .pdf .page .content .foreword_detail p {
     line-height: 0.35rem;
     text-indent: 2em;
     font-size: 0.23rem;
     color: #333;
     margin-bottom: 0.08rem;
}
 .evaluation_report .pdf .page .content .foreword_table {
     padding-top: 0.16rem;
     margin-bottom: 0.24rem;
}
 .evaluation_report .pdf .page .content .foreword_table table {
     width: 4.8rem;
     margin: 0 auto;
}
.evaluation_report .pdf .page .content .foreword_table table tr td {
     line-height: 0.42rem;
     font-size: 0.2rem;
     text-align: center;
     border: 2px solid #fff;
     color: #168275;
     background-color: #CCE4E1;
}
.evaluation_report .content .foreword_footer {
  padding: 0.2rem 0.3rem;
  background-color: #edf3ff;
  border-radius: 0.1rem;
}
.evaluation_report .pdf .page .content .foreword_footer .title {
  line-height: 0.48rem;
  margin-bottom: 0.04rem;
  font-size: 0.28rem;
  font-weight: 600;
  color: #3c91f4;
  padding-left: 0.6rem;
  background: url(../img/diagnosis/icon_tishi.jpg) no-repeat 0rem center;
  background-size: 0.32rem 0.48rem;
}
.evaluation_report .pdf .page .content .foreword_footer p {
     font-size: 0.24rem;
     line-height: 0.36rem;
     margin-bottom: 0.04rem;
}
 .evaluation_report .pdf .page .content .warmtips_header {
     font-size: 0.22rem;
     line-height: 0.35rem;
     margin-bottom: 0.16rem;
}
 .evaluation_report .pdf .page .content .tips_box {
     margin-bottom: 0.24rem;
}
 .evaluation_report .pdf .page .content .tips_box .tips_title {
     font-size: 0.22rem;
     color: #168375;
     line-height: 0.35rem;
     margin-bottom: 0.04rem;
}
 .evaluation_report .pdf .page .content .tips_box p {
     line-height: 0.35rem;
     font-size: 0.22rem;
     color: #333;
}
 .evaluation_report .pdf .page .content .resule_header {
     font-size: 0.24rem;
     line-height: 0.4rem;
}
 .evaluation_report .pdf .page .content .resule_table {
     margin-bottom: 0.32rem;
}
 .evaluation_report .pdf .page .content .resule_table ul li {
     margin-bottom: 0.16rem;
}
 .evaluation_report .pdf .page .content .resule_table ul li .lis_title {
     line-height: 0.28rem;
     font-size: 0.22rem;
     margin-bottom: 0.04rem;
}
 .evaluation_report .pdf .page .content .resule_table ul li .lis_title span {
     display: inline-block;
     width: 0.28rem;
     height: 0.28rem;
     margin-right: 0.08rem;
     line-height: 0.28rem;
     text-align: center;
     border-radius: 50%;
     font-size: 0.16rem;
     color: #fff;
     background-color: #168275;
}
 .evaluation_report .pdf .page .content .resule_table ul li .lis_deatil {
     padding-left: 0.256rem;
     font-size: 0.2rem;
}
 .evaluation_report .pdf .page .content .echarts_box .title {
     margin-bottom: 0.16rem;
     font-weight: 600;
     font-size: 0.22rem;
}
 .evaluation_report .pdf .page .content .echarts_box .fenxi {
     margin-bottom: 0.08rem;
     font-size: 0.2rem;
}
 .evaluation_report .pdf .page .content .echarts_box .fenxi span {
     color: #168375;
     font-weight: 600;
     font-size: 0.2rem;

}
 .evaluation_report .pdf .page .big_page_number {
     position: absolute;
     bottom: 0rem;
     left: 0rem;
     width: 100%;
     line-height: 0.32rem;
     font-size: 0.16rem;
     font-weight: 600;
     color: #666;
     text-align: center;
}
 