_this.$router.push('/login');\r\n                }\r\n            })\r\n            .catch(function(error) {\r\n                console.log(error);\r\n                // alert(error)\r\n            });\r\n        },\r\n        getUserInfo() {\r\n            var _this = this;\r\n            var token =  window.sessionStorage.getItem('ymtxToken');\r\n            this.$ajax\r\n                // get请求\r\n                .post(this.G_uri + '/login/getUserInfo', {\r\n                    // data\r\n\r\n                },{\r\n                    headers:{\r\n                        token: token\r\n                    }\r\n                })\r\n                // 监听数据返回\r\n                // .then(({ data }) => console.log(data))\r\n                .then(function(res) {\r\n                    // console.log(res)\r\n                    if(res.data.code == 2000) {\r\n                        // 如果设置了省\r\n                        if(res.data.data.province) {\r\n                            _this.province = res.data.data.province;\r\n                            _this.province0 = res.data.data.province;\r\n                            if(res.data.data.city) {\r\n                                _this.city = res.data.data.city;\r\n                                _this.city0 = res.data.data.city;\r\n                                if(res.data.data.area) {\r\n                                    _this.area = res.data.data.area;\r\n                                    _this.area0 = res.data.data.area;\r\n                                }\r\n\r\n                            }\r\n                        }\r\n                        if(res.data.data.baccll_year) {\r\n                            _this.year = res.data.data.baccll_year;\r\n                            _this.year0 = res.data.data.baccll_year;\r\n                        }\r\n                        if(res.data.data.school_name) {\r\n                            _this.school0 = res.data.data.school_name;\r\n                            _this.school = res.data.data.school_name;\r\n                        }\r\n                        _this.start=res.data.data.school_class_start;\r\n                        _this.end=res.data.data.school_class_end;\r\n\r\n                    }\r\n                })\r\n                .catch(function(error) {\r\n                    console.log(error)\r\n                })\r\n        },\r\n        //获取最低高考年份\r\n        getYear(){\r\n            var date=new Date();\r\n            var year=date.getFullYear();\r\n            var mounth=date.getMonth()+1;\r\n            var day=date.getDate();\r\n            if(mounth>=9){\r\n                year+=1\r\n            }\r\n            this.years=year;\r\n            this.year=year;\r\n        }\r\n\r\n    },\r\n    mounted() {\r\n        // this.getUserInfo();\r\n        // var orderHight = document.documentElement.clientHeight || document.body.clientHeight;\r\n        // if(orderHight < 720) {\r\n        //     this.isShowScoll = true;\r\n        //  // document.getElementById('logins').style.height = orderHight + 'px';\r\n        // }\r\n        if(window.sessionStorage.tuance=='3'){\r\n            this.tuance=true\r\n        }\r\n\r\n    },\r\n    created:function(){\r\n        this.getYear();\r\n        this.getCityData();\r\n        this.getProvance();\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n","import mod from \"-!../../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../../node_modules/thread-loader/dist/cjs.js!../../../../node_modules/babel-loader/lib/index.js!../../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./PerfectInformation.vue?vue&type=script&lang=js&\"; export default mod; export * from \"-!../../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../../node_modules/thread-loader/dist/cjs.js!../../../../node_modules/babel-loader/lib/index.js!../../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../../no