      }
      .arrow-ani {
        // opacity: 0;
        animation: arrowAni 1s;
      }
      @keyframes arrowAni {
        0% {
          z-index: -10;
        }
        50% {
          z-index: -10;
        }
        99.9999% {
          z-index: -10;
        }
        100% {
          z-index: 10;
        }
