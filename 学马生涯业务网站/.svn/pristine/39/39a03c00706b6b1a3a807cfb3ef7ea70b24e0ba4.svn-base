
.evalu-title {
  color: #394148;
  font-size: 35px;
  height: 8.333%;
  letter-spacing: 3px;
  position: absolute;
  top: 0;
  left: 13.54%;
  text-indent: 2rem;
  display: flex;
  align-items: center;
}

.evalu-nav {
  width: 100%;
  height: 70px;
  position: absolute;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.3);
  li {
    width: 7.69%;
    height: 70px;
    text-align: center;
    box-sizing: border-box;
    border-right: 1px solid #999;
    font-weight: bold;
    color: #999;
    float: left;
    p {
      width: 100%;
      text-align: center;
      line-height: 26px;
      font-size: 17px;
      span {
        line-height: 20px;
        font-size: 14px;
        display: block;
        width: auto;
      }
      span:first-child {
        margin-left: -35px;
      }
      span:last-child {
        margin-right: -35px;
      }
      span.center {
        margin: 0;
        text-align: center;
      }
    }
  }
  .xljk-nav {
    a.router-link-active {
      background: #e7e9ec;
      display: block;
      color: #669966;
    }
  }
  .xqqx-nav {
    a.router-link-active {
      background: #e7e9ec;
      display: block;
      color: #cc3366;
    }
    p:first-child {
      font-size: 18px;
    }
    p:last-child {
      border-bottom: 2px solid #cc3366;
    }
  }
  .rzqn-nav {
    position: relative;
    border-bottom: 2px solid #cc6600;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    align-items: center;
    color: #fff;
    min-height: 65px;
    box-sizing: border-box;
    padding-bottom: 5px;
    text-align: center;
    width: 100%;
    p:first-child {
      font-size: 14px;
      padding-top: 0;
      width: 100%;
    }
    p:last-child {
      //border-bottom: 2px solid #cc6600;
      height: auto;
      border-bottom: 0;
      font-size: 16px;
      line-height: 16px;
      width: 100%;

    }
  }
  .rzqn-nav.router-link-active {
    background: #e7e9ec;
    color: #ff6600;
  }
  @media screen and (max-width: 1640px) {
    li p {
      font-size: 14px !important;
      span:first-child {
        font-size: 12px;
        margin-left: -20px;
      }
      span:last-child {
        font-size: 12px;
        margin-right: -20px
      }
    }
  }
  @media screen and (max-width: 1440px) {
    li p {
      font-size: 13px !important;
      span:first-child {
        font-size: 11px;
        margin: 0
      }
      span:last-child {
        font-size: 11px;
        margin: 0
      }
    }
  }
}

