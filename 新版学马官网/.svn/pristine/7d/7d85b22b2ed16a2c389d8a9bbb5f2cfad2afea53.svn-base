.part3 {
  background: url("../imgs/home/icon_diquan.png") center center no-repeat;
  background-color: #F5F9FC;
  height: 918px;
}
.part3 .items {
  position: relative;
  width: 1040px;
  margin: 0 auto;
  border: 1px solid transparent;
}
.part3 .items li {
  position: absolute;
  width: 170px;
  height: 70px;
}
.part3 .items li p {
  font-size: 18px;
  margin-bottom: 6px;
  font-weight: bold;
}
.part3 .items li span {
  font-size: 14px;
  color: #999;
  line-height: 18px;
}
.part3 .items .li1 {
  left: 50px;
  top: 140px;
}
.part3 .items .li2 {
  left: 0;
  top: 414px;
}
.part3 .items .li3 {
  left: 416px;
  top: 670px;
}
.part3 .items .li4 {
  right: 40px;
  top: 140px;
}
.part3 .items .li5 {
  right: 0;
  top: 414px;
}
.part3 .items .canvas {
  position: absolute;
  width: 590px;
  height: 586px;
  border: 1px solid transparent;
  left: 224px;
  top: 15px;
  z-index: 9;
}
.part3 .items .circle {
  width: 28px;
  height: 28px;
  border-radius: 50%;
  position: absolute;
  background: #F5F9FC;
  z-index: 99;
}
.part3 .items .circle1 {
  left: 308px;
  top: 112px;
}
.part3 .items .circle2 {
  left: 254px;
  top: 385px;
}
.part3 .items .circle3 {
  left: 493px;
  top: 560px;
}
.part3 .items .circle4 {
  left: 698px;
  top: 112px;
}
.part3 .items .circle5 {
  left: 751px;
  top: 385px;
}
.part3 .items .women {
  display: block;
  margin: 30px auto;
  position: relative;
  top: 35px;
  transform: scale(0.95);
}
.part4 {
  background: url("../imgs/home/kecheng_bg.png") center center no-repeat;
  width: 100%;
  height: 630px;
  position: relative;
}
.part4 .zhichilis {
  width: 1200px;
  display: flex;
  justify-content: space-between;
  padding-top: 56px;
}
.part4 .zhichilis li {
  width: 280px;
  height: 380px;
  border-radius: 15px;
  background: #fff;
}
.part4 .zhichilis li img {
  display: block;
  margin: 70px auto 30px;
}
.part4 .zhichilis li p {
  text-align: center;
  font-size: 18px;
  font-weight: bold;
}
.part4 .zhichilis li .p {
  padding: 10px 40px;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.part4 .zhichilis li .p span {
  font-size: 14px;
  font-weight: normal;
  color: #999999;
  display: block;
  line-height: 26px;
}
.part5 .users {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-top: 36px;
  padding-bottom: 30px;
}
.part5 .users li {
  box-shadow: 0 0 5px #E3E3E3;
  width: 285px;
  height: 270px;
  margin-bottom: 22px;
}
.part5 .users li .img {
  width: 100%;
  height: 190px;
}
.part5 .users li p {
  text-align: center;
  line-height: 78px;
  font-size: 14px;
}
.part5 .users li:nth-of-type(1) .img {
  background: url("../imgs/home/use_xuema/icon_longnan.png") center center no-repeat;
}
.part5 .users li:nth-of-type(2) .img {
  background: url("../imgs/home/use_xuema/icon_ronghua.png") center center no-repeat;
}
.part5 .users li:nth-of-type(3) .img {
  background: url("../imgs/home/use_xuema/icon_tianxin.png") center center no-repeat;
}
.part5 .users li:nth-of-type(4) .img {
  background: url("../imgs/home/use_xuema/icon_liaochengdiyizhongxue.png") center center no-repeat;
}
.part5 .users li:nth-of-type(5) .img {
  background: url("../imgs/home/use_xuema/icon_pingyuanxian.png") center center no-repeat;
}
.part5 .users li:nth-of-type(6) .img {
  background: url("../imgs/home/use_xuema/icon_shandongshiyan.png") center center no-repeat;
}
.part5 .users li:nth-of-type(7) .img {
  background: url("../imgs/home/use_xuema/icon_sichuanmianyang.png") center center no-repeat;
}
.part5 .users li:nth-of-type(8) .img {
  background: url("../imgs/home/use_xuema/icon_longnan.png") center center no-repeat;
}
.part6 {
  height: 494px;
  width: 100%;
  background: #F5F9FC;
}
.part6 .hezuo {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.part6 .hezuo li {
  width: 230px;
  height: 84px;
  background: #fff;
  margin-top: 28px;
}
.part6 .hezuo li img {
  width: 100%;
  height: 100%;
}
