 .banner{
    width: 100%;
    height: 704px;
    padding-top: 121px;
    background: url("../imgs/aboutus/banner.png") center center;
    color:#fff;
}
 .banner *{
     color:#fff;
 }
 .banner .head{
     font-size: 82px;
     line-height: 82px;
     text-align: center;
     margin-bottom: 60px;
 }
 .banner p{
     font-size: 22px;
     width: 1200px;
     margin: 0 auto;
     line-height: 38px;
     letter-spacing: 1.5px;
     text-indent: 48px;
 }
 .inner{
     /*height: 770px;*/
     width: 100%;
     /*background: #fff;*/
 }
 .content{
     height: 770px;
     width: 1200px;
     margin: 0 auto;
     background: #fff;
     position: relative;
 }
 .tops{
     width: 100%;
     height: 152px;
     border-radius: 10px;
     background: #fff;
     position: relative;
     top:-70px;
     box-shadow:0 20px 20px #F5F5F5;
 }
 .tops>li{
     width: 25%;
     height: 100%;
     text-align: center;
     float: left;
     font-size: 18px;
     color:#666666;
 }
 .tops>li>p{
     font-size: 18px;
     padding:30px 0 18px;
 }
 .tops>li>span{
     font-size: 45px;
     font-weight: bolder;
     color:#3186F7;
     letter-spacing: 0;
 }
 .tops>li>a{
     color:#666;
     position: relative;
     top:-2px;
 }
 .middle{
     display: flex;
     justify-content: space-between;
 }
 .middle>li{
     width: 330px;
 }
 .middle>li>.top{
     font-size:24px;
     color:#3381F4;
     text-align: center;
     font-weight: bolder;
 }
 .middle>li>.line{
     width: 32px;
     height: 5px;
     border-radius: 3px;
     background: #3189F8;
     margin: 14px auto;
 }
 .middle>li>p{
     font-size: 15px;
     line-height: 28px;
 }
 .part3{
     width: 100%;
     background: url("../imgs/aboutus/bg_shijian.png") top center/100% 100% no-repeat;
 }
 .parts{
     width: 1100px;
     margin:0 auto;
     padding-bottom:50px;
 }
 .title{
     font-size: 32px;
     text-align: center;
     font-weight: 600;
     padding: 55px 0;
 }
 .licheng{
     position: relative;
 }
 .licheng>li{
     margin-bottom: 18px;

 }
 .licheng>li:after{
     clear: both;
     content: "";
     display: block;
 }
.licheng>li>div{
   float: left;
}
.lis1{
    width: 250px;
}
 .lis1>span{
     font-size: 36px;
     font-weight: bolder;
     color:#3282F4;
     letter-spacing: 0;
 }
 .lis1>a{
     font-size: 20px;
 }
 .lis2{
     padding-top: 14px;
     margin-right: 50px;
     position: relative;
 }
 .lis2>img{
     display: block;
 }
 .lis2>.line{
     width: 2px;
     background: #CBCBCB;
     position: absolute;
     top:38px;
     left:11px;
 }
 .lis3{
     padding-top: 14px;
     max-width: 774px;
 }
 .lis3>p{
     font-size: 16px;
     margin-bottom: 16px;
     letter-spacing: 1.5px;
 }
 .persons{
     display: flex;
     justify-content: space-between;
 }
 .persons>li{
    width: 336px;

 }
 .persons>li>img{
     display: block;
     margin: 0 auto;
     position: relative;
     z-index: 3;
 }
 .box{
     width: 100%;
     height: 430px;
     border:1px solid #D5D4D4;
     box-shadow:0 0  10px #EAEAEA;
     position: relative;
     top:-70px;
     padding: 0 22px;
 }
 .names{
     width: 126px;
     height: 40px;
     background: #3186F7;
     line-height: 40px;
     text-align: center;
     color:#fff;
     margin:108px auto 40px;
     font-size: 20px;
 }
 .zhiwei{
     text-align: center;
     font-size:18px;
     margin-bottom: 22px;
 }
 .box>p{
     text-align: center;
     color: #999;
     line-height: 26px;
     font-size: 13px;
 }
 .bottom{
     height: 290px;
     background:url("../imgs/aboutus/lijilianxi.png") center center;
 }
.title2{
    font-size: 46px;
    text-align: center;
    font-weight: 500;
    padding: 60px 0 48px;
    color:#fff;
}
.liji{
    width: 240px;
    height: 62px;
    border-radius: 31px;
    margin: 0 auto;
    text-align: center;
    line-height: 62px;
    font-size: 22px;
    background: #EB3634;
    color:#fff;
    cursor: pointer;
    position: relative;
    box-shadow: 1px  5px  5px #BD0617;
}


 .fixed_box {
     position: fixed;
     left: 0;
     bottom: 0;
     width: 100%;
     height: 64px;
     box-sizing: border-box;
     padding: 12px 0;
     z-index: 10;
     background-color: #1B1A1A;
 }
 .fixed_box .btn_close {
     position: absolute;
     width: 24px;
     height: 24px;
     background-color: #fff;
     line-height: 24px;
     color: #333;
     text-align: center;
     border-radius: 12px;
     right: 20px;
     top: 19px;
     cursor: pointer;
 }
 .fixed_box .select_box {
     position: relative;
 }
 .fixed_box .select_box img {
     position: absolute;
     right: 15px;
     top: 15px;
     width: 16px;
     height: 11px;
 }
 .fixed_box .select_box .img_active {
     transform: rotate(-180deg);
     transition: all .5s;
 }
 .fixed_box .select_box .option_box {
     position: absolute;
     bottom: 40px;
     left: 0px;
     border: 1px solid #dedede;
     border-radius: 10px;
     background-color: #fff;
     width: 174px;
     padding: 5px 0px;
 }
 .fixed_box .select_box .option_box p {
     line-height: 30px;
     cursor: pointer;
     padding-left: 10px;
     color: #333;
 }
 .fixed_box .select_box .option_box p:hover {
     color: #F2473D;
 }
 .fixed_box ul {
     display: flex;
     float: left;
 }
 .fixed_box ul li {
     margin-right: 15px;
     display: flex;
     font-size: 14px;
     color: #333;
 }
 .fixed_box ul li p {
     line-height: 40px;
     color: #fff;
 }
 .fixed_box ul li input,
 .fixed_box ul li textarea,
 .fixed_box ul li select {
     border: 1px solid #D2D3D3;
     box-sizing: border-box;
     padding: 0px 10px;
     outline:none
 }
 .fixed_box ul li input,
 .fixed_box ul li select {
     width: 174px;
     height: 40px;
     line-height: 40px;
     border-radius: 5px;
 }
 .fixed_box ul li textarea {
     height: 40px;
     line-height: 40px;
     width: 270px;
     border-radius: 5px;
 }
 .fixed_box .btn {
     float: left;
     width: 125px;
     height: 40px;
     background-color: #E7252E;
     text-align: center;
     line-height: 40px;
     border-radius: 5px;
     color: #fff;
     cursor: pointer;
 }
 .jiameng{
     width: 1200px;
     margin: 0 auto;
 }
 input::-webkit-outer-spin-button,
 input::-webkit-inner-spin-button {
     -webkit-appearance: none;
 }
 input[type="number"]{
     -moz-appearance: textfield;
 }
 .fixed_box{
     display: none
 }
.swiper-container{
    margin: 55px auto 0 !important;
}
 .swiper-slide{
     width: 80%;
 }
.sw{
    width: 855px;
    height: 330px;
    margin: 0 auto;
}
.sw1{
    background: url(../imgs/aboutus/icon_01.png) no-repeat center center;
}
.sw2{
    background: url(../imgs/aboutus/icon_02.png) no-repeat center center;
}
.sw3{
    background: url(../imgs/aboutus/icon_03.png) no-repeat center center;
}
.swiper-pagination{
    left:50%;
    width: 166px;
    display: flex;
    justify-content: space-between;
    bottom:40px;
    margin-left: -83px;
}
 .swiper-pagination-bullet{
    width: 45px;
     height: 14px;
     border-radius: 7px;
     outline: none;
 }
