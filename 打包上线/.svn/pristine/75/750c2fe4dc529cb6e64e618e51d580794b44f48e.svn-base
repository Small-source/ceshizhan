       this.drawTotal();\r\n                    this.drawPart();\r\n                }\r\n            },\r\n            //各科成绩图表/总名次\r\n            getPartChart(){\r\n                let _this=this;\r\n                this.$ajax.post(this.G_uri + this.ports.schoolWorkMessage.getPartChart, {\r\n                    grade:1\r\n                },{\r\n                    headers:{\r\n                        token:window.sessionStorage.getItem('ymtxToken')\r\n                    }\r\n                }).then(res=>{\r\n                    this.gradePartData=res.data.data.testData.reverse();\r\n                })\r\n            },\r\n            //各科成绩变化图表\r\n            drawPart(){\r\n                let xArr=[],yArr=[],zArr=[],\r\n                    subject=['语文','数学',\"外语\",'物理','化学','生物','历史','思想政治','地理','技术']\r\n                for(var i=0;i<10;i++){\r\n                    let object={\r\n                        name:subject[i],\r\n                        type:'line',\r\n                        data:[]\r\n                    }\r\n                    let object2={\r\n                        name:subject[i],\r\n                        type:'line',\r\n                        data:[]\r\n                    }\r\n                    this.gradePartData.slice(this.gradeTotalIndex*6,this.gradeTotalIndex*6+6).forEach((item)=>{\r\n                        if(i==0){\r\n                            xArr.push({\r\n                                value:item.date+'\\n'+item.name\r\n                            })\r\n                        }\r\n                        object.data.push(item.subject[i].score)\r\n                        object2.data.push(item.subject[i].rank)\r\n                    })\r\n                    yArr.push(object)\r\n                    zArr.push(object2)\r\n                }\r\n                xArr.reverse();\r\n                yArr.reverse();\r\n                zArr.reverse();\r\n                // 基于准备好的dom，初始化echarts实例\r\n                let myChart = this.$ec