* {
    margin: 0;
    padding: 0;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%;
}
ul, li {
	list-style: none;
}
a {
	text-decoration: none;
}
body {
    background: #F3F4F8;
}
.main_inner{
    width: 1200px;
    margin: 20px auto;
}
#loading {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 100000000;
    background-color: #fff;
}
#loading .card {
	position: absolute;
	left: 50%;
    top: 50%;
    height: 80px;
    width: 80px;
    margin-top: -40px;
    margin-left: -40px;
    background-image: url(/career/imgs/loading/loading1.png);
    background-repeat:  no-repeat;
    background-position: center center;
    background-size: 100% 100%;
    animation: rotate 1.5s linear infinite;
    z-index: 100000000;
}
@keyframes rotate{
    from{
        transform: rotate(360deg)
    }
    to{
        transform: rotate(0deg)
    }
}
/*下载进度条*/
.downloadProgressMark{
	position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 10000;
    background: rgba(0,0,0,.5);
}
.downloadProgressMark .progressBar {
 	width: 330px;
    height: 100px;
    position: fixed;
    left: 50%;
    top: 50%;
    margin-top: -50px;
    margin-left: -165px;
    z-index: 99;
}
.downloadProgressMark .progressBar .box1{
    width: 100%;
    height: 30px;
    border-radius: 30px;
    background:#D9D9D9;
    position: relative;
    overflow: hidden; 
}
.downloadProgressMark .progressBar .box1 .jincheng{
    position: absolute;
    top: 0;
    right:100%;
    width: 100%;
    height: 100%;
    background-image:url("/career/imgs/careerManagement/academicPlanningReport/jindutiao.png");
    background-size:100% 100%;
}
.downloadProgressMark .progressBar .box2{
    text-align: center;
    color:#fff;
    line-height: 40px;
    font-size: 25px;
}
.zhutiLeft{
    box-sizing: border-box;
    width: 210px;
    float: left;
    margin-right: 20px;
    background: #fff;
    box-shadow: 0 0 3px #dedede;
    padding: 10px 0 10px 10px;
}
.el-menu-vertical-demo:not(.el-menu--collapse) {
    width: 200px;
    min-height: 800px;
}
.el-menu{
    border-right: none;
}
.zhutiLeft a{
    color:#333;
}