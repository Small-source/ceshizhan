        .biao {
            font-size: 32px;
            margin-bottom: 40px;
        }
        .biao_box {
            height: 311px;
            background: url(../imgs/zhaoshanghezuo/icon_shijianzhou.png) no-repeat center center;
            position: relative;
            li {
                position: absolute;
                text-align: center;
                &:nth-child(1) {
                    left: 130px;
                    top: -15px;
                }
                &:nth-child(2) {
                    left: 344px;
                    bottom: -20px;
                }
                &:nth-child(3) {
                    left: 560px;
                    top: -15px;
                }
                &:nth-child(4) {
                    left: 767px;
                    bottom: -20px;
                }
                &:nth-child(5) {
                    left: 1025px;
                    top: 70px;
                }
                .title {
                    font-size: 18px;
                    font-weight: 600;
                    color: #E33E49;
                    line-height: 40px;
                }
                .year {
                    .title;
                    font-size: 22px;
                }
                .dian {
                    height: 50px;
                }
                .detail {
                    line-height: 24px;
                    font-size: 16px;
                    color: #000;
                }
            }
        }
    }
