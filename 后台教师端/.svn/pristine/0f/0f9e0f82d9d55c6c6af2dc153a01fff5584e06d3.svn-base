*{
    margin: 0;
    padding: 0;
    list-style: none;
    text-decoration: none;
    outline: none;
}
input{
    border:none
}
body{
    font-size: 14px;
}
.main{
    width: 1200px;
    margin: 30px auto;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%;
}
#masks{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
    background: rgba(0,0,0,.5);
}
.Cards .el-input__inner{
    height: 38px;
    border:1px solid #DEDEDE;
}
.Cards .el-select{
    margin-right: 20px;
}
.el-input{
    display: inline-block;
}
.balance .el-input__inner{
    width: 120px;
    height: 30px;
}
.balance .el-icon-arrow-up{
    line-height: 30px !important;
}
#loading {
    position: fixed;
    left: 50%;
    top: 50%;
    height: 80px;
    width: 80px;
    margin-top: -40px;
    margin-left: -40px;
    background-image: url(/career/imgs/loading/loading1.png);
    background-repeat:  no-repeat ;
    background-position: center center;
    background-size: 100% 100%;
    animation: rotate 1.5s linear infinite;
    z-index: 999;
}
@keyframes rotate{
    from{
        transform: rotate(360deg)
    }
    to{
        transform: rotate(0deg)
    }
}
img{
    display: inline-block;
    max-width: 100%;
    vertical-align: middle;
}
body{
    background:#F3F4F8;
}
.zhutiLeft{
    box-sizing: border-box;
    width: 210px;
    float: left;
    background: #fff;
    box-shadow: 0 0 3px #dedede;
    padding: 10px 0 10px 10px;
}
.el-menu-vertical-demo:not(.el-menu--collapse) {
    width: 200px;
    min-height: 800px;
}
.el-menu{
    border-right: none;
}
.zhutiLeft a{
    color:#333;
}
.el-submenu .el-menu-item {
    padding: 0 0 !important;
    width: 200px !important;
}
.el-submenu .el-menu-item a{
    display: block;
    width: 100%;
    height: 100%;
    text-indent: 40px;
}
