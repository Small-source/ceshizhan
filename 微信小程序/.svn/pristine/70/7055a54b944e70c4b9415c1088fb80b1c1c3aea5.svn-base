/* pages/professionalPositioningAssessment/professionalPositioningAssessment.wxss */
@import  "../public/public.wxss";
.head{
  position: relative;
  padding: 0 30rpx;
  height:234rpx;
}
.head>image{
  position: absolute;
  width: 100%;
  height:234rpx;
  left:0;
  top:0;
}
.head .name{
  height: 120rpx;
  line-height: 120rpx;
  font-size: 40rpx;
  color:#FFFFFF;
  font-weight: bold;
}
.label .image1{
  width: 30rpx;
  height: 28rpx;
  position: relative;
  top:6rpx;
}
.label .image2{
  width: 33rpx;
  height: 28rpx;
  position: relative;
  top:6rpx;
}
.label .image3{
  width: 25rpx;
  height: 28rpx;
  position: relative;
  top:6rpx;
}
.label>view{
  float: left;
  margin-right:40rpx;
  font-size: 28rpx;
  color:#fff;
}
.label>view>view{
  font-size: 22rpx;
  padding-left: 40rpx;
  padding-top: 10rpx;
}
.content {
  background-color:#F5F5F5;
  color: #333;
}
.content .titleBox {
  /* box-sizing: border-box; */
  display: flex;
  height: 90rpx;
  align-items: center;
  margin-bottom: 20rpx;
  justify-content: space-between;
}
.content .titleBox navigator {
  color: #9a9a9a;
  font-size: 22rpx;
  display: flex;
  align-items: center;
}
.content .titleBox navigator image {
  width: 17rpx;
  height: 23rpx;
  margin-left: 5rpx;
}
.content .title {
  border-left: 6rpx solid #3979FD;
  font-size: 34rpx;
  line-height: 34rpx;
  height: 34rpx;
  font-weight: 600;
  padding-left: 10rpx;
  background-color: #fff;
}
.content .main {
  background-color: #fff;
  margin-bottom: 10rpx;
  padding: 0 30rpx;
}
.content .detail {
  color: #9a9a9a;
  font-size: 26rpx;
}
.detailBox{
  margin-bottom: 6rpx solid #e5e5e5;
  padding-bottom: 30rpx;
  overflow: hidden
}
.xiangjin{
  font-size: 28rpx;
  height: 52rpx;
  line-height: 52rpx;
  padding: 0 10rpx;
  border:2rpx solid #F2F2F2;
  float: left;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.xiangjin_hover{
  color:#739EFD;
  border-color:#739EFD;
}
.topBox{
  background: #F5F5F5;
  height: 72rpx;
}
.topBox>view{
  width: 33.3%;
  float: left;
  height: 100%;
  font-size: 26rpx;
  color:#333333;
  font-weight: bold;
  text-align: center;
  line-height: 72rpx;
}
.detailBox2{
  background:"#FFFFFF";
  border-bottom: 2rpx solid #F2F2F2;
  overflow: hidden;
  position: relative;
  height: 72rpx;
}
.detailBox2>view{
  width: 33.3%;
  float: left;
  font-size: 24rpx;
  color:#3E3E3E;
  text-align: center;
  line-height: 72rpx;
  height: 72rpx;
}
.detailBox2 image{
  width: 14rpx;
  height: 21rpx;
  position: absolute;
  top: 28rpx;
  z-index: 99;
  right:10rpx;
  margin-bottom: 10rpx;
}
.save_box {
  height: 100rpx;
  width: 100%;
  background-color: #3979FD;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
}
.save_box image {
  width: 58rpx;
  height: 58rpx;
  margin-right: 15rpx;
}
.save_box view {
  display: flex;
  align-items: center;
}