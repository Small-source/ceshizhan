.head{
  background: url("../imgs/home/banner.png") center center no-repeat;
  width: 100%;
  height: 540px;
}
.parts{
  width: 1200px;
  margin: 0 auto;
}
.part1{
  background: url("../imgs/home/icon_shengxuefuwu.png") center center no-repeat;
  width: 100%;
  height: 880px;
  .tops{
    width: 100%;
    height: 150px;
    border-radius: 20px;
    background: #fff;
    position: relative;
    top:-54px;
    box-shadow:0 20px 20px #F5F5F5;
  }
  .tops>li{
    width: 20%;
    height: 100%;
    text-align: center;
    float: left;
    font-size: 18px;
    color:#666666;
    padding-top:25px;
    cursor: pointer;
  }
  .tops>li:hover span{
    transform: scale(1.2);
    //font-size: 50px;
  }
  .tops>li>p{
    font-size: 18px;
    padding:10px 0 18px;
    color:#666666;
    text-align: left;
    text-indent: 57px;
  }
  .tops>li>span{
    font-size: 45px;
    font-weight: bolder;
    color:#3186F7;
    letter-spacing: 0;
    display: inline-block;
    transition: .1s;
  }
  .tops>li>a{
    color:#666;
    position: relative;
    top:-2px;
  }
  .dajiantou{
    display: block;
    margin: 46px auto 0;
  }
}
.title{
  font-size: 33px;
  text-align: center;
  font-weight: 600;
  span{
    font-size: 18px;
    color:#fff;
  }
}
.xueye{
  width: 100%;
  height: 320px;
  border-bottom:1px solid #FFFFFF;
  background: url("../imgs/home/icon_lanbeijing.png") center center no-repeat;
}
.xueyelist{
  width: 954px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  height: 100%;
  li{
    width: 225px;
    height: 100%;
    position: relative;
    p{
      text-align: center;
      line-height: 76px;
      font-size: 26px;
      color:#fff;
    }
    .p{
      position: absolute;
      left: 0;
      bottom:-76px;
      color: #333;
      font-size: 20px;
      width: 100%;
    }
  }
}
.part2{
  background: url("../imgs/home/bg_chanpintixi.png") center center no-repeat;
  width: 100%;
  height: 946px;
  .part2box{
    width: 1050px;
    height: 710px;
    margin: 40px auto 0;
    background: url("../imgs/home/icon_diannao.png") center 21px no-repeat;
    position: relative;
    .pli{
      position: absolute;
      width: 210px;
      height: 38px;
      background: url("../imgs/home/icon_xiantiao.png") center bottom no-repeat;
      color:#fff;
      font-size: 20px;
      top: 0;
    }
    .pli2{
      top:124px;
      left: 0;

    }
    .pli3{
      top:545px;
      left: 0;
    }
    .pli4{
      top:668px;
      left: 0;
    }
    .pli5{
      right:0;
    }
    .pli6{
      right:0;
      top:124px;
    }
    .pli7{
      right:0;
      top:190px
    }
    .pli8{
      right:0;
      top:550px;
    }
    .mo{
      position: absolute;
      left:0;
      top:0;
      font-size: 20px;
      color:#3276E9;
      font-weight: bold;
    }
    .mo1{
      left: 130px;
      top:70px;
    }
    .mo2{
      left: 130px;
      top:615px;
    }
    .mo3{
      left: 834px;
      top:70px;
    }
    .mo4 {
      left: 834px;
      top: 615px;
    }
    .mo5{
      left: 236px;
      top: 490px;
      color:#fff;
      font-size: 14px;
      width: 80px;
      text-align: center;
      line-height: 32px;
    }
    .mo6{
      left: 466px;
      top: 467px;
      color:#fff;
      font-size: 14px;
      width: 106px;
      text-align: center;
      line-height: 32px;
    }
    .mo7{
      left: 645px;
      top: 488px;
      color:#fff;
      font-size: 14px;
      width: 158px;
      text-align: center;
      line-height: 32px;
    }
  }
}
.part3{
  background:url("../imgs/home/icon_diquan.png") center center no-repeat;
  background-color:#F5F9FC;
  height:918px;
  .items{
    position: relative;
    width: 1040px;
    margin: 0 auto;
    border:1px solid transparent;
    li{
      position: absolute;
      width: 170px;
      height: 70px;
      p{
        font-size: 18px;
        margin-bottom: 6px;
        font-weight: bold;
      }
      span{
        font-size: 14px;
        color:#999;
        line-height: 18px;
      }
    }
    .li1{
      left:50px;
      top:140px;
    }
    .li2{
      left:0;
      top:414px;
    }
    .li3{
      left: 416px;
      top: 670px;
    }
    .li4{
      right:40px;
      top:140px;
    }
    .li5{
      right:0;
      top:414px;
    }
    .canvas{
      position: absolute;
      width: 590px;
      height: 586px;
      border:1px solid transparent;
      left:224px;
      top:15px;
      z-index: 9;
    }
    .circle{
      width: 28px;
      height: 28px;
      border-radius: 50%;
      position: absolute;
      background: #F5F9FC;
      z-index: 99;
    }
    .circle1{
      left: 308px;
      top:112px;
    }
    .circle2{
      left: 254px;
      top:385px;
    }
    .circle3{
      left: 493px;
      top:560px;
    }
    .circle4{
      left: 698px;
      top:112px;
    }
    .circle5{
      left: 751px;
      top:385px;
    }
    .women{
      display: block;
      margin: 30px auto 0;
      position: relative;
    }
  }
}
.part4{
  background: url("../imgs/home/kecheng_bg.png") center center no-repeat;
  width: 100%;
  height: 630px;
  position: relative;
  .zhichilis{
    width: 1200px;
    display: flex;
    justify-content: space-between;
    padding-top: 56px;
    li{
      width: 280px;
      height: 380px;
      border-radius: 15px;
      background: #fff;
      img{
        display: block;
        margin: 70px auto 30px;
      }
      p{
        text-align: center;
        font-size: 18px;
        font-weight: bold;
      }
      .p{
        padding: 10px 40px;
        display: flex;
        justify-content: space-between;
        flex-wrap: wrap;
        span{
          font-size: 14px;
          font-weight: normal;
          color:#999999;
          display: block;
          line-height: 26px;
        }
      }
    }
  }
}
.part5{
  .users{
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    padding-top: 36px;
    padding-bottom: 30px;
    li{
      box-shadow: 0 0 5px #E3E3E3;
      width: 285px;
      height: 270px;
      margin-bottom: 22px;
      .img{
        width: 100%;
        height: 190px;
      }
      p{
        text-align: center;
        line-height: 78px;
        font-size: 14px;
      }
      &:nth-of-type(1) .img{
        background: url("../imgs/home/use_xuema/icon_longnan.png") center center no-repeat;
      }
      &:nth-of-type(2) .img{
        background: url("../imgs/home/use_xuema/icon_ronghua.png") center center no-repeat;
      }
      &:nth-of-type(3) .img{
        background: url("../imgs/home/use_xuema/icon_tianxin.png") center center no-repeat;
      }
      &:nth-of-type(4) .img{
        background: url("../imgs/home/use_xuema/icon_liaochengdiyizhongxue.png") center center no-repeat;
      }
      &:nth-of-type(5) .img{
        background: url("../imgs/home/use_xuema/icon_pingyuanxian.png") center center no-repeat;
      }
      &:nth-of-type(6) .img{
        background: url("../imgs/home/use_xuema/icon_shandongshiyan.png") center center no-repeat;
      }
      &:nth-of-type(7) .img{
        background: url("../imgs/home/use_xuema/icon_sichuanmianyang.png") center center no-repeat;
      }
      &:nth-of-type(8) .img{
        background: url("../imgs/home/use_xuema/icon_longnan.png") center center no-repeat;
      }
    }
  }
}
.part6{
  height: 494px;
  width: 100%;
  background: #F5F9FC;
  .hezuo{
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    li{
      width: 230px;
      height: 84px;
      background: #fff;
      margin-top: 28px;
      img{
        width: 100%;
        height: 100%;
      }
    }
  }
}
