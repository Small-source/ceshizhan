.xgtx-biao2 {
  height: 400px;
  margin-top: 20px !important;
  background: url("~@/assets/pdf/xgtx-bg2.png") no-repeat left 108px / 100% auto;
  .biao-content {
    top: 118px !important;
  }
  .biao-font {
    top: 109px !important;
  }
  .title {
    width: 400px;
    margin: 0 auto 10px;
    overflow: hidden;
    .title-item {
      float: left;
      width: 50%;
      box-sizing: border-box;
      height: 80px;
      h3 {
        font-size: 15px;
        width: 120px;
        height: 30px;
        margin-bottom: 10px;
        float: left;
        line-height: 30px;
      }
      h3.active {
