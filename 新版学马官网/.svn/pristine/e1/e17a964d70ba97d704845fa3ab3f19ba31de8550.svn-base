.huhangjihua .our_advantage {
  padding-bottom: 100px;
  background-color: #fff;
}
.huhangjihua .our_advantage .title {
  margin-bottom: 30px;
}
.huhangjihua .our_advantage .content {
  background: url(../imgs/huhangjihua/bg_youshi.png) no-repeat center center;
  height: 326px;
}
.huhangjihua .our_advantage .content ul {
  display: flex;
  justify-content: space-between;
  height: 366px;
  position: relative;
  top: -20px;
}
.huhangjihua .our_advantage .content ul li {
  box-sizing: border-box;
  width: 240px;
  height: 366px;
  padding-top: 70px;
  color: #fff;
  text-align: center;
}
.huhangjihua .our_advantage .content ul li .small_title {
  line-height: 60px;
  text-align: center;
  font-size: 20px;
}
.huhangjihua .our_advantage .content ul li .line {
  width: 95px;
  border: 1px solid #fff;
  margin: 0 auto;
}
.huhangjihua .our_advantage .content ul li .small_detail {
  padding: 0 40px;
  padding-top: 25px;
  font-size: 14px;
  text-align: left;
}
.huhangjihua .our_advantage .content ul li .img_box {
  height: 40px;
}
.huhangjihua .our_advantage .content ul li:nth-child(1) .img_box {
  background: url(../imgs/huhangjihua/icon_xianshang.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:nth-child(2) .img_box {
  background: url(../imgs/huhangjihua/icon_jiejuefangan.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:nth-child(3) .img_box {
  background: url(../imgs/huhangjihua/icon_shizi.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:nth-child(4) .img_box {
  background: url(../imgs/huhangjihua/icon_genzong.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:nth-child(5) .img_box {
  background: url(../imgs/huhangjihua/icon_pingtai.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:hover {
  position: relative;
  z-index: 2;
  box-shadow: 0px 0px 35px rgba(167, 169, 180, 0.2);
  background-color: #fff;
  color: #333;
}
.huhangjihua .our_advantage .content ul li:hover .small_title {
  color: #333;
}
.huhangjihua .our_advantage .content ul li:hover .line {
  border: 1px solid #377BED;
}
.huhangjihua .our_advantage .content ul li:hover .small_detail {
  color: #333;
}
.huhangjihua .our_advantage .content ul li:hover:nth-child(1) .img_box {
  background: url(../imgs/huhangjihua/icon_xianshanglan.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:hover:nth-child(2) .img_box {
  background: url(../imgs/huhangjihua/icon_jiejuefanganlan.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:hover:nth-child(3) .img_box {
  background: url(../imgs/huhangjihua/icon_shizilan.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:hover:nth-child(4) .img_box {
  background: url(../imgs/huhangjihua/icon_genzonglan.png) no-repeat center center;
}
.huhangjihua .our_advantage .content ul li:hover:nth-child(5) .img_box {
  background: url(../imgs/huhangjihua/icon_pingtailan.png) no-repeat center center;
}
.huhangjihua .fuwuneirong {
  padding-bottom: 100px;
  background-color: #f5f5f5;
}
.huhangjihua .fuwuneirong .lis {
  width: 686px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  margin-bottom: 35px;
}
.huhangjihua .fuwuneirong .lis li {
  line-height: 42px;
  border: 1px solid #347CF0;
  color: #347CF0;
  font-size: 18px;
  text-align: center;
  border-radius: 21px;
  width: 138px;
  cursor: pointer;
}
.huhangjihua .fuwuneirong .lis .active {
  background-color: #347CF0;
  color: #fff;
}
