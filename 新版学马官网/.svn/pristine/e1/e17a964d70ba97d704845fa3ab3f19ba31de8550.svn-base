.huhangjihua .table_box {
  height: 421px;
  padding: 40px 30px;
  background-color: #fff;
}
.huhangjihua table {
  border-collapse: collapse;
  border-spacing: 0;
  width: 100%;
}
.huhangjihua table .bg_blue {
  color: #347CDD;
  font-weight: bold;
}
.huhangjihua table tbody {
  display: none;
}
.huhangjihua table .active {
  display: table-row-group;
}
.huhangjihua table tr th {
  line-height: 50px;
  font-size: 16px;
  color: #fff;
  border: 1px solid #fff;
  background-color: #3184F5;
  text-align: center;
}
.huhangjihua table tr th:nth-child(1) {
  width: 290px;
}
.huhangjihua table tr th:nth-child(2) {
  width: 350px;
}
.huhangjihua table tr td {
  line-height: 40px;
  border: 1px solid #fff;
  background-color: #E3F2FF;
  color: #333;
  font-size: 14px;
  text-align: center;
}
.huhangjihua table tr td img {
  width: 20px;
  height: 20px;
}
.huhangjihua .inner {
  width: 1200px;
  margin: 0 auto;
}
.huhangjihua .title {
  height: 130px;
  line-height: 130px;
  font-size: 32px;
  font-weight: bold;
  color: #333;
  text-align: center;
}
.huhangjihua .title_detail {
  text-align: center;
  margin-top: -30px;
}
.huhangjihua .banner img {
  width: 100%;
}
.huhangjihua .qunti {
  background-color: #F4F6F8;
  padding-bottom: 50px;
}
.huhangjihua .qunti .main_box {
  display: flex;
  justify-content: space-between;
}
.huhangjihua .qunti .main_box li {
  position: relative;
  width: 366px;
}
.huhangjihua .qunti .main_box li p {
  position: absolute;
  bottom: 20px;
  left: 0;
  text-align: center;
  width: 100%;
}
.huhangjihua .jianjie {
  background-color: #fff;
  padding-bottom: 50px;
  font-size: 18px;
  line-height: 36px;
}
.huhangjihua .jianjie .detail_box {
  padding: 60px 0;
  position: relative;
}
.huhangjihua .jianjie .detail_box p {
  margin-bottom: 20px;
  position: relative;
}
.huhangjihua .jianjie .detail_box .img01 {
  position: absolute;
  left: 0;
  top: -20px;
}
.huhangjihua .jianjie .detail_box .img02 {
  position: absolute;
  right: 0;
  bottom: 50px;
  transform: rotate(180deg);
}
.huhangjihua .grade {
