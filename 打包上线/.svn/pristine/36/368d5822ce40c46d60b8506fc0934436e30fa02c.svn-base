* {
  box-sizing: border-box;
}
body {
  color: #333;
}
.voluntaryReport {
  padding-bottom: 1.14rem;
}
.voluntaryReport .logo_box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 0.3rem;
  height: 1.2rem;
  font-size: 0.24rem;
}
.voluntaryReport .logo_box img {
  display: block;
  width: 1.6rem;
}
.voluntaryReport .big_title {
  padding-left: 0.3rem;
  color: #fff;
  font-size: 0.36rem;
  font-weight: bold;
}
.voluntaryReport .small_title {
  height: 0.34rem;
  display: flex;
  align-items: center;
  font-size: 0.32rem;
  font-weight: bold;
  color: #E72630;
}
.voluntaryReport .small_title img {
  width: 0.39rem;
  height: 0.33rem;
}
.voluntaryReport .banner {
  width: 7.5rem;
  height: 5.82rem;
}
.voluntaryReport .banner img {
  width: 100%;
  height: 100%;
}
.voluntaryReport .service_products {
  padding: 1.1rem 0.3rem 0.5rem 0.3rem;
  background-color: #fff;
}
.voluntaryReport .service_products .content {
  padding: 0 0.3rem;
  background-color: #FFEEEE;
  border-radius: 0.2rem;
}
.voluntaryReport .service_products .content .main_box {
  padding: 0.55rem 0 0.4rem 0;
  border: 1px dashed #F3969B;
}
.voluntaryReport .service_products .content .main_box .lis_products {
  width: 2.03rem;
  height: 2.23rem;
  padding-top: 1.7rem;
}
.voluntaryReport .service_products .content .main_box .lis_products:nit-child(1) {
  background: url(../imgs/investment/icon_xuemashengya.png) no-repeat;
  font-size: 100% 100%;
}
.voluntaryReport .service_products .content .main_box .lis_products:nit-child(2) {
  background: url(../imgs/investment/icon_shengyafazhanguanlipingtai.png) no-repeat;
  font-size: 100% 100%;
}
.voluntaryReport .service_products .content .main_box .lis_products:nit-child(3) {
  background: url(../imgs/investment/icon_xiaochengxu.png) no-repeat;
  font-size: 100% 100%;
}
