  justify-content: space-around;
  font-size: 20rpx;
}
.biaoLeft{
  width: 140rpx;
  height: 204rpx;
  float: left;
  line-height: 36rpx;
  display: flex;
  align-content: space-around;
  flex-wrap: wrap;
}
.biaoLeft view{
   font-size: 20rpx;
   width: 100rpx;
   text-align: center;
   margin:  0 auto;
   height: 36rpx;
}
.score{
  width: 157rpx;
}
.score>view{
  position: relative;
  width: 100%;
  height: 25%;
}
.score>view>view{
  position: absolute;
  top:8rpx;
  height: 36rpx;
  background: #99148d
}
.title2{
  text-align: center;
  font-size: 24rpx;
  font-weight: bolder;
  margin-top: 10rpx;
}
.xgtxList>view{
  border-bottom:2rpx dashed #D8D8D8;
  padding-left:30rpx;
  position: relative;
  line-height: 50rpx;
  font-size: 24rpx;
  color:#7F7F7F;
  padding: 20rpx 0 20rpx 30rpx;
}
.circle{
  position: absolute;
  left:8rpx;
  top:40rpx;
  width: 10rpx;
  height: 10rpx;
  border-radius:50%;
  background:#1856D6;
}
.active{
    background: #99148d;
    color: #fff;
}
/* 表格代码   */
.table{
  border:1px solid #dadada;
  border-right:0;
  border-bottom: 0;
  width: 98%;
  margin-left: 1%;
}
.tr{
  width:100%;
  overflow: hidden;
  /* display: flex;
  justify-content: space-between; */
}
.th,.td{
  padding: 20rpx 8rpx;
  border-bottom: 2rpx solid #dadada;
  border-right: 2rpx solid #dadada;
  text-align: center;
  width: 100%;

}
.th1,.th2,.td1,.td2{
  width: 15% !important;
  
}
.th{
  font-weight: 800;
  background-color: #E3F0FF;
  font-size: 14rpx;
  color: #67AFF5;
  float: left;
  width:7.5%;
}
.thView{
  text-align: center;
}
.tds{
  float: left;
  width: 75%;
  min-height: 80rpx;
  background: none;
  position: relative;
}
.tds .zifen,.tds .changgui{
  position: absolute;
  top:20rpx;
  left:0;
  height: 40rpx;
  background: #2D87FB;
  width: 40rpx;
  z-index: 0;
}
.tds>.changgui2{
  background:#000;
  z-index: 10;
  opacity: 0.2;
}
