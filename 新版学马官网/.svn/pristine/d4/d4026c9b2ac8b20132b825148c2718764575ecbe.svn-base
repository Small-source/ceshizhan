    .bimianshi {
        background-color: #f9f9f9;
        .inner {
            width: 1200px;
            margin: 0 auto;
        }
        .banner {
            width: 100%;
            height: 480px;
            background: url(../imgs/bimianshi/banner01.png) no-repeat center center;
        }
        .content {
            background: #F5F5F5;
            padding-bottom: 40px;
            // 复试网课
                .inners {
                    // margin-top: -50px;
                    top: -84px;
                    background-color: #fff;
                    padding: 10px 25px;
                    padding-top: 10px;
                    padding-bottom: 20px;
                    width: 1150px;
                    margin: 0 auto;
                    box-shadow: 0px 0px 8px rgba(222, 222, 222, .61);
                    position: relative;
                    border-radius: 5px;
                    // z-index: 200;
                }
                .title {
                    height: 42px;
                    line-height: 42px;
                    color: #333;
                    font-size: 22px;
                    font-weight: bold;
                    letter-spacing: 0.1em;
                }
                .bigTitle {
                    height: 38px;
                    line-height: 38px;
                    color: #676767;
                    font-weight: 500;
                    font-size: 15px;
                    letter-spacing: 1.5px;
                }
                .jianjie {
                    height: 36px;
                    line-height: 36px;
                    color: #E13F01;
                    font-weight: 600;
                    font-size: 16px;
                    letter-spacing: 1.5px;
                }
                .footLis {
                    padding-top: 14px;
                    height: 36px;
                    li {
                        float: left;
                        width: 86px;
                        height: 30px;
                        text-align: center;
                        line-height: 30px;
                        margin-right: 20px;
                        border: 1px solid #de675f;
                        color: #de675f;
                        font-size: 14px;
                        border-radius: 3px;
                        letter-spacing: 1px;
                    }
                }
                .goBuy {
                    position: absolute;
                    right: 20px;
                    bottom: 25px;
                    width: 160px;
                    height: 44px;
                    color: #fff;
                    background-color: #3592f9;
                    border-radius: 5px;
                    line-height: 44px;
                    text-align: center;
                    font-size: 15px;
                    letter-spacing: 1.5px;
                    cursor: pointer;
                    box-shadow: 0px 3px 3px #135fb3;
                    &:active {
                        // background-color: #156def;
                        box-shadow: none;
                    }
                }
                .havenBuy {
                    .goBuy;
                }
            // 学马介绍
            .xuemaIntroduction {
                position: relative;
                top: -45px;
                .inner {
                    padding-top: 20px;
                }
                .title {
                    height: 70px;
                    color: #333;
                    line-height: 70px;
                    text-align: center;
                    font-size: 22px;
                    font-weight: bold;
                }
                .xiangqing {
                    // padding: 5px 20px;
                    font-size: 14px;
                    color: #666;
                    p {
                        text-indent: 2em;
                    }
                }
            }
            // 课程大纲
            .courseOutline {
                // margin-bottom: 20px;
                background-color: #fff;
                .inner {
                    width: 100%;
                    background-color: #fff;
                    padding: 0 20px;
                    box-sizing: border-box;
                }
                .title {
                    height: 40px;
                    color: #333;
                    line-height: 40px;
                    text-align: center;
                    font-size: 22px;
                    font-weight: bold;
                }
                .OutlineContet {
                    // padding: 0px 20px 20px 20px;
                }
                .analysis {
                    font-size: 14px;
                    color: #666;
                    line-height: 30px;
                    margin-bottom: 14px;
                    text-align: center;
                }
                .description {
                    line-height: 24px;
                    font-size: 14px;
                    color: #666;
                }

                .details {
                    padding-top: 20px;
                    table {
                        text-align: center;
                        border: 1px solid #dedede;
                        tr {
                            width: 1160px;
                            line-height: 54px;
                            th {
                                padding-left: 20px;
                                text-align: center;
                                width: 556px;
                                color: #333;
                                font-size: 18px;
                                background: #DCDCDC;
                            }
                        }
                        tr {
                            width: 100%;
                            line-height: 45px;
                            td {
                                padding-left: 20px;
                                width: 556px;
                                font-size: 14px;
                            }
                        }
                    }

                }
            }
            // 学员评价
            .sudentEvaluation {
                margin-bottom: 20px;
                .inner {
                    padding-top: 15px;
                    background-color: #fff;
                }
                .title {
                    width: 94px;
                    height: 36px;
                    background-color: #3592f9;
                    border-radius: 0px 18px 18px 0px;
                    color: #fff;
                    padding-left: 20px;
                    line-height: 36px;
                }
                .lis {
                    padding: 20px;
                    overflow: hidden;
                    li {
                        padding: 5px 20px;
                        width: 278px;
                        height: 259px;
                        border: 1px solid #d9d9d9;
                        border-radius: 2px;
                        float: left;
                        &:last-child {
                            margin-right: 0px;
                        }
                        .topBox {
                            padding: 12px 0px;
                            .imgBox {
                                float: left;
                                margin-right: 15px;
                                height: 60px;
                                width: 60px;
                                border-radius: 50%;
                                img {
                                    height: 60px;
                                    width: 60px;
                                    border-radius: 50%;
                                }
                            }
                            .topRight {
                                float: left;
                                .name {
                                    font-size: 14px;
                                    color: #333;
                                    height: 25px;
                                    line-height: 25px;
                                    margin-bottom: 7px;
                                }
                                .address {

                                    height: 28px;
                                    line-height: 28px;
                                    font-size: 12px;
                                    color: #3492f9;
                                }
                            }

                        }
                        .description {
                            padding: 20px;
                            overflow: hidden;
                        }
                    }
                }
            }
            // 讲师介绍
            .instructors {
                // margin-bottom: 20px;
                // height: 448px;
                .title {
                    height: 110px;
                    color: #fff;
                    line-height: 110px;
                    text-align: center;
                    font-size: 22px;
                }
                .lis {
                    padding: 0 20px 50px;
                    display: flex;
                    justify-content: space-between;
                    // overflow: hidden;
                    li {
                        box-sizing: border-box;
                        padding-bottom: 0px;
                        width: 276px;
                        height: 420px;
                        margin-right: 20px;
                        float: left;
                        position: relative;
                        transition: all .5s linear;
                        &:last-child {
                            margin-right: 0px;
                        }
                        img {
                            width: 100%;
                            height: 100%;
                        }
                        .teacherBox{
                            position: absolute;
                            bottom: 0;
                            left: 0;
                            width: 245px;
                            height: 90px;
                            padding-left: 15px;
                            padding-right: 15px;
                            background: rgba(53,146,249,0.9);
                            .teacher {
                                font-size: 20px;
                                font-weight: bolder;
                                color: #fff;
                                line-height: 40px;
                            }
                            .university {
                                font-size: 14px;
                                line-height: 18px;
                                color: #fff;
                            }
                        }

                    }
                }
            }
            .contentBox{
                position: relative;
                top:-63px;
                width: 1200px;
                margin: 0 auto;
                background: #fff;
                overflow: hidden;
                .title{
                    line-height: 76px;
                    padding-left: 20px;
                    height: 76px;
                    display: flex;
                    align-items: center;
                    span{
                        font-size: 20px;
                        color:#3592F9;
                        font-weight: bolder;
                        margin-left: 14px;
                        letter-spacing: 2px;
                    }
                }
                .listnavs{
                    width: 100%;
                    height: 64px;
                    border-bottom: 1px solid #EAEAEA;
                    padding-top: 17px;
                    box-sizing: border-box;
                    .listnavBox{
                        width: 384px;
                        margin: 0 auto;
                        display: flex;
                        justify-content: space-between;
                        li{
                            width: 88px;
                            height: 30px;
                            border-radius: 15px;
                            background: #E6E7E9;
                            color:#3592FA;
                            text-align: center;
                            line-height: 30px;
                            letter-spacing: 2px;
                            font-size: 14px;
                            font-weight: bolder;
                            cursor: pointer;
                        }
                    }
                }
                // 复试网课
                .videoBox1 {
                    width: 100%;
                    padding: 0 20px;
                    margin-bottom: 40px;
                    box-sizing: border-box;
                    .list{
                        overflow: hidden;
                        width: 100%;
                    }
                    .inner {
                        padding: 0px 0px;
                        box-sizing: border-box;
                        width: 100%;
                    }
                    .videoBox_left {
                        float: left;
                        width: 670px;
                        height: 386px;
                        background-color: #c1defe;
                        border-radius: 3px;
                        padding: 8px;
                        box-sizing: border-box;
                        video {
                            width: 100%;
                            height:100%;
                        }
                    }
                    .videoBox_right {
                        float: left;
                        padding-top: 27px;
                        padding-left: 50px;
                        background: url(../imgs/bimianshi/icon/icon_ren.png) no-repeat 430px 28px;
                        .title {
                            width: 100%;
                            font-size: 18px;
                            line-height: 54px;
                            color: #3592f9;
                            border-bottom: 1px solid #dcdcdc;
                            margin-bottom: 10px;
                            padding-left: 0;
                            letter-spacing: 2px;
                            font-weight: bolder;
                        }
                        .tese {
                            font-size: 16px;
                            color: #333;
                            line-height: 36px;
                            font-weight: 600;
                        }
                        .detail {
                            line-height: 34px;
                            color: #666;
                            font-size: 14px;
                        }
                        .fuli {
                            line-height: 70px;
                            color: #333;
                            font-weight: bold;
                            font-size: 14px;
                            padding-left: 30px;
                            background: url(../imgs/bimianshi/icon/icon_liwu.png) no-repeat 0px center;
                        }
                    }
                }
            }
        }
    }