.page {
  box-shadow: 0px 0px 10px #666;
  min-height: 1164rpx;
  padding-top: 170rpx;
  background: url(https://m.xuematech.com/applet/img/diagnosis/reportbgp.png) no-repeat;
  background-size: 100% 100%;
  background-color: #fff;
  position: relative;
  margin-bottom: 16rpx;
}
.page_cover {
  height: 1334rpx;
  padding-top: 314rpx;
  background-color: #fff;
  background: url(https://m.xuematech.com/applet/img/diagnosis/reportbgp.png) no-repeat;
  background-size: 100% 100%;
  box-sizing: border-box;
  box-shadow: 0rpx 0rpx 8rpx #666;
  margin-bottom: 16rpx;
}
.page_cover .header {
  margin-bottom: 220rpx;
}
.page_cover .header .h3 {
  margin: 0;
  text-align: center;
  line-height: 66rpx;
  font-size: 54rpx;
  font-weight: bold;
  color: #168375;
  margin-bottom: 24rpx;
}
.page_cover .header .detail {
  line-height: 40rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}
.page_cover .main {
  line-height: 48rpx;
  font-size: 26rpx;
  font-weight: 600;
  color: #333;
  width: 400rpx;
  margin: 0 auto;
  margin-bottom: 240rpx;
}
.page_cover .main .name {
  display: flex;
  justify-content: center;
}
.page_cover .main view {
  line-height: 48rpx;
  font-size: 26rpx;
  font-weight: 600;
  color: #333;
}
.page_cover .cover_warning {
  padding: 0 30rpx;

}
.page_cover .cover_warning view {
  line-height: 32rpx;
  padding-left: 35rpx;
  font-size: 24rpx;
  margin-bottom: 16rpx;
}
.page_cover .cover_warning image {
  float: left;
  margin-left: -35rpx;
  width: 30rpx;
  height: 30rpx;
}
.page .s_header {
  text-align: center;
  color: #168375;
  font-size: 56rpx;
  line-height: 56rpx;
  margin-bottom: 35rpx;
  font-weight: normal;
} 
.page .content {
  padding: 24rpx 30rpx;
  padding-top: 0;
}
.page .content .warmtips_header {
  font-size: 24rpx;
  line-height: 35rpx;
  margin-bottom: 16rpx;
}
.page .content .tips_box {
  margin-bottom: 24rpx;
}
.page .content .tips_box .tips_title {
  font-size: 24rpx;
  color: #168375;
  line-height: 35rpx;
  margin-bottom: 4rpx;
  font-weight: bold;
}
.page .content .tips_box view {
  line-height: 35rpx;
  font-size: 24rpx;
  color: #333;
}
.page .content .big_title_box {
  display: flex;
  line-height: 28rpx;
  margin-bottom: 30rpx;
}
.page .content .big_title_box .big_title {
  position: relative;
  font-size: 28rpx;
  color: #000;
  padding-right: 4px;
}
.page .content .big_title_box .big_title text {
  height: 28rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #168375;
  font-weight: bold;
  padding-right: 40rpx;
}
.page .content .big_title_box .line {
  flex: 1;
  border-bottom: 1px dashed #000;
  margin-bottom: 2px;
}
.page .content .big_title_box .page_number {
  padding-left: 8rpx;
  font-size: 24rpx;
}
.page .content .foreword_detail {
  margin-bottom: 30rpx;
}
.page .content .foreword_detail view {
  line-height: 35rpx;
  text-indent: 2em;
  font-size: 24rpx;
  color: #333;
  margin-bottom: 8rpx;
}
.page .content .foreword_footer {
  padding: 20rpx 30rpx;
  background-color: #edf3ff;
  border-radius: 10rpx;
}
.page .content .foreword_footer .title {
  line-height: 48rpx;
  margin-bottom: 4rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #3c91f4;
  padding-left: 60rpx;
  background: url(https://m.xuematech.com/applet/img/diagnosis/icon_tishi.jpg) no-repeat 0rpx center;
  background-size: 32rpx 48rpx;
}
.page .content .foreword_footer view {
  font-size: 24rpx;
  line-height: 36rpx;
  margin-bottom: 4rpx;
}
.page .content .foreword_table {
  padding-top: 16rpx;
  width: 484rpx;
  margin: 0 auto;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.page .content .foreword_table view {
  line-height: 42rpx;
  font-size: 22rpx;
  text-align: center;
  color: #168275;
  background-color: #CCE4E1;
  margin-bottom: 4rpx;
  width: 240rpx;
}

